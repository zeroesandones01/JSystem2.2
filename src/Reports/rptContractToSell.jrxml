	<parameter name="pbl_id" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["241"]]></defaultValueExpression>
	</parameter>
	<parameter name="seq_no" class="java.lang.Integer" isForPrompting="false">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT --sp_center_format(30, FORMAT('%s %s', TRIM(c.first_name), TRIM(c.last_name))) as client_name,
sp_center_format(0, TRIM(f.company_name)) as company_name,
f.company_address,
sp_center_format(0, TRIM(d.proj_name)) as proj_name,
UPPER(FORMAT('%s, %s, %s', TRIM(d.barangay), TRIM(d.municipality), TRIM(d.province))) as proj_address,
sp_center_format(0, FORMAT('%s %s', TRIM(c.first_name), TRIM(c.last_name))) as client_name,
sp_center_format(0, COALESCE(NULLIF(TRIM(FORMAT('%s %s', TRIM(h.first_name), TRIM(h.last_name))), ''), 'N/A')) as client_spouse,
sp_center_format(0, COALESCE(get_client_address(a.entity_id), 'N/A')) as client_address,
sp_center_format(0, COALESCE(NULLIF(TRIM(FORMAT('%s %s', TRIM(j.first_name), TRIM(j.last_name))), ''), 'N/A')) as coapplicant,
sp_center_format(0, COALESCE(NULLIF(TRIM(FORMAT('%s %s', TRIM(l.first_name), TRIM(l.last_name))), ''), 'N/A')) as coapplicant_spouse,
sp_center_format(0, COALESCE(get_client_address(i.entity_id), 'N/A')) as coapplicant_address,

b.phase, b.block, b.lot, b.lotarea, p.floor_area, e.model_desc, a.sellingprice as gsp, m.disc_amount as discount, COALESCE(m.vat, 0) as vat, (a.sellingprice - m.disc_amount) as nsp,
o.client_id_no, o.client_id_presented, o.client_tin_no, o.signatory_name, o.signatory_id_presented, o.signatory_id_no, o.signatory_tin_no, o.signatory_position

FROM rf_sold_unit a
LEFT JOIN mf_unit_info b ON b.proj_id = a.projcode and b.pbl_id = a.pbl_id
LEFT JOIN rf_entity c ON c.entity_id = a.entity_id
LEFT JOIN mf_project d ON d.proj_id = a.projcode
LEFT JOIN mf_product_model e ON e.model_id = a.model_id
LEFT JOIN mf_company f ON f.co_id = d.co_id

--Spouse
LEFT JOIN rf_entity_connect g ON g.entity_id = a.entity_id and g.connect_type = 'SP'
LEFT JOIN rf_entity h ON h.entity_id = g.connect_id

--Co-Applicant
LEFT JOIN rf_entity_connect i ON i.entity_id = a.entity_id and i.connect_type = 'CO'
LEFT JOIN rf_entity j ON j.entity_id = i.connect_id

--Co-Applicant Spouse
LEFT JOIN rf_entity_connect k ON k.entity_id = i.entity_id and k.connect_type = 'SP'
LEFT JOIN rf_entity l ON l.entity_id = k.connect_id

LEFT JOIN rf_ihf_computation m ON m.entity_id = a.entity_id AND m.proj_id = a.projcode AND m.pbl_id = a.pbl_id AND m.seq_no = a.seq_no

LEFT JOIN (SELECT * FROM view_acknowledgement_part($P{entity_id}, $P{proj_id}, $P{pbl_id}, $P{seq_no})) o
ON o.entity_id = a.entity_id --AND o.proj_id = a.projcode AND o.pbl_id = a.pbl_id AND o.seq_no = a.seq_no

LEFT JOIN mf_product_model p ON p.model_id = a.model_id

WHERE a.entity_id = $P{entity_id}
AND a.projcode = $P{proj_id}
AND a.pbl_id = $P{pbl_id}
AND a.seq_no = $P{seq_no};]]>
	</queryString>
