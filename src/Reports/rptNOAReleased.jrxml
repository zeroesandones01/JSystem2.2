LEFT JOIN mf_unit_info b ON b.proj_id = a.proj_id AND b.pbl_id = a.pbl_id
LEFT JOIN mf_project c ON c.proj_id = a.proj_id
LEFT JOIN rf_sold_unit d ON d.entity_id = a.entity_id AND d.projcode = a.proj_id AND d.pbl_id = a.pbl_id AND d.seq_no = a.seq_no
WHERE a.byrstatus_id = '35'
ORDER BY client_name, getinteger(b.phase), gettext(b.phase), getinteger(b.block), gettext(b.block), getinteger(b.lot), gettext(b.lot);]]>
