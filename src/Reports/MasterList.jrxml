	<queryString>
		<![CDATA[select * from (select
a.asset_code,
a.item_id,
c.category_name,
a.asset_name,
a.asset_cost,
a.asset_ulm,
a.asset_mon_dep,
to_char(a.asset_no,'FM00000000') as asset_no,
a.asset_serial,
a.asset_model,
a.asset_bk_val,
a.asset_ret_cost,
a.date_acquired,
a.from_dep,
a.to_dep,
a.date_retired,
a.owned,
a.insured,
a.insured_value,
coalesce(i.remarks, a.remarks) as remarks,
(substr(a.status, 1, 1)) as status,
format('%s, %s %s.',e.last_name, e.first_name, left(e.middle_name,1)) as current_cust,
--format('%s/%s/%s',g.division_alias, f.dept_alias, h.loc_alias) as divdeptbrach,
g.division_alias,
f.dept_alias,
j.supp_name,
c.category_id
from tbl_asset a
left join tbl_item b on a.item_id=b.item_id
left join tbl_category c on b.category_id=c.category_id
left join em_employee d on a.current_cust=d.emp_code::int
left join rf_entity e on d.entity_id=e.entity_id
left join mf_department f on d.dept_code=f.dept_code
left join mf_division g on f.division_code = g.division_code or d.div_code=g.division_code
left join rf_location h on d.loc_id=h.loc_id
left join tbl_asset_history i on a.asset_no=i.asset_no and a.current_cust=i.current_cust
left join tbl_supplier j on a.supp_id=j.supp_id
where   a.status not in('INACTIVE','I')
and a.co_id not in('3')
and a.asset_bk_val > 0.00
and (b.category_id::varchar=$P{category_id} or $P{category_id}='All')
and (g.division_code=$P{div_id} or $P{div_id}='All' )
and (f.dept_code=$P{dept_id} or $P{dept_id}='All')
and (a.supp_id::varchar=$P{supp_id} or $P{supp_id}='All')
and d.emp_code not in ('null')
--order by g.division_alias, f.dept_alias
) a
where current_cust is not null
order by division_alias,dept_alias,(case when $P{sort_by} = 'current_cust' then current_cust else  asset_no end),(case when $P{sort_by}= 'current_cust' then asset_no  end  ) ;]]>
	</queryString>
