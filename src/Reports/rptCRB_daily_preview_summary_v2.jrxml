	<queryString>
		<![CDATA[select *
from
(
	select a.corollary, a.acct_id, a.proj_alias, a.phase, a.acct_name, sum(a.debit) as debit, sum(a.credit) as credit,
	'To record the collection for ' || $P{cdr_date}::DATE || ', ' ||
	(select branch_alias || ' BRANCH' from  mf_office_branch where branch_id = $P{branch}) as remarks,
	(case when corollary then 1::int else 0::int end) as sort
	from
	(
		select (case when x.acct_id in ('03-02-14-000', '03-02-18-000', '03-02-02-000', '03-02-19-000', '11-02-00-000') or (x.acct_id = '03-02-16-000' and sum(x.credit) > 0) or (x.acct_id = '01-02-01-000' and sum(x.credit) > 0) or (x.acct_id = '01-02-14-000' and x.loan_released = false) then false else (y.dbt_cdt = 'C' and sum(x.debit) > 0) or exists(select * from mf_boi_chart_of_accounts z where z.acct_id = x.acct_id and z.corollary) end) as corollary, x.acct_id, x.proj_alias, x.phase, x.acct_name, sum(x.debit) as debit, sum(x.credit) as credit
		from tmpcrb x
		inner join mf_boi_chart_of_accounts y on x.acct_id = y.acct_id
		where x.debit::numeric(19, 2) > 0::numeric(19, 2)
		and x.user_id = $P{user_id}
		and x.co_id = $P{co_id}
		and x.branch_id = $P{branch}
		and x.cdr_date::DATE = $P{cdr_date}
		group by x.acct_id, x.proj_alias, x.phase, x.acct_name, y.dbt_cdt, x.loan_released
		union
		select (case when x.acct_id in ('01-02-99-005', '03-02-18-000', '03-02-02-000', '03-02-19-000', '11-02-00-000', '03-02-16-000') or (x.acct_id = '01-02-01-000' and sum(x.credit) > 0) or (x.acct_id = '01-02-14-000' and x.loan_released = false) then false else exists(select * from mf_boi_chart_of_accounts z where z.acct_id = x.acct_id and z.corollary) end) as corollary, x.acct_id, x.proj_alias, x.phase, x.acct_name, sum(x.debit) as debit, sum(x.credit) as credit
		from tmpcrb x
		inner join mf_boi_chart_of_accounts y on x.acct_id = y.acct_id
		where x.credit::numeric(19, 2) > 0::numeric(19, 2)
		and x.user_id = $P{user_id}
		and x.co_id = $P{co_id}
		and x.branch_id = $P{branch}
		and x.cdr_date::DATE = $P{cdr_date}
		group by x.acct_id, x.proj_alias, x.phase, x.acct_name, y.dbt_cdt, x.loan_released
	) a
	group by a.corollary, a.acct_id, a.proj_alias, a.phase, a.acct_name
) a
order by (case when corollary then 1::int else 0::int end), a.debit desc, a.credit asc;]]>
