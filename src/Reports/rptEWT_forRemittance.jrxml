	<queryString>
		<![CDATA[select *
from
(
select bb.entity_id2 as entity_id,
cc.tin_no, upper(trim(c.entity_name)) as client,
a.rplf_no, d.cv_no, '' as jv_no, b.rplf_date, a.wtax_amt::DECIMAL as wtax_amt,
(coalesce(a.exp_amt,0) - coalesce(a.wtax_amt,0))::DECIMAL as net_exp_amt,
e.date_paid,
(case when a.sub_projectid is not null or a.sub_projectid = '' then true else
(case when f.lts_date is null then false else true end) end) as with_lts,
(SELECT SUM(x.tran_amt) FROM rf_pv_detail x WHERE x.pv_no = d.pv_no AND status_id = 'A' AND bal_side = 'D') as Debit
from (select * from rf_request_detail where status_id != 'I') a
left join (select * from rf_request_header where status_id != 'I') b on a.rplf_no =b.rplf_no and a.co_id = b.co_id
left join (select * from rf_pv_header where status_id != 'I') bb on a.rplf_no =bb.rplf_no and a.co_id = bb.co_id
left join rf_entity c on (case when trim(b.rplf_type_id) = '04' then (a.entity_id) else trim(bb.entity_id2) end) = c.entity_id
left join rf_entity_id_no cc on  a.entity_id = cc.entity_id
left join (select * from rf_pv_header where status_id != 'I') d on b.rplf_no = d.rplf_no and b.co_id = d.co_id
join (select * from rf_cv_header where status_id = 'P') e on d.cv_no = e.cv_no and d.co_id = e.co_id
left join (select * from mf_sub_project where status_id != 'I') f on a.sub_projectid = f.sub_proj_id

where a.wtax_amt > 0
and bb.pv_date >= '2014-01-01 00:00:00'
and a.co_id = $P{co_id}
and (case when $P{year} = 'All' then a.rplf_no is not null else trim(to_char(bb.pv_date, 'yyyy')) = $P{year} end)
and (case when $P{period} = '0' then a.rplf_no is not null else
	case when $P{period} = '1' then substr(trim(to_char(bb.pv_date, 'MM-dd-yyyy')),0,3) in ('01','02','03') else
	case when $P{period} = '2' then substr(trim(to_char(bb.pv_date, 'MM-dd-yyyy')),0,3) in ('04','05','06') else
	case when $P{period} = '3' then substr(trim(to_char(bb.pv_date, 'MM-dd-yyyy')),0,3) in ('07','08','09') else
	case when $P{period} = '4' then substr(trim(to_char(bb.pv_date, 'MM-dd-yyyy')),0,3) in ('10','11','12') else a.rplf_no is not null
	end end end end end)
and (case when $P{month} = 'All' then a.rplf_no is not null else substr(trim(to_char(bb.pv_date, 'MM-dd-yyyy')),0,3) = $P{month} end)
and (case when $P{payee_id} = '' then a.rplf_no is not null else (case when trim(b.rplf_type_id) = '04' then (a.entity_id) else trim(b.entity_id1) end) = $P{payee_id} end)
and (case when $P{rplf_no} = 'All' then a.rplf_no is not null else a.rplf_no = $P{rplf_no} end)
and (case when $P{entity_type_id} = '' then a.rplf_no is not null else b.entity_type_id = $P{entity_type_id} end)
and (case when $P{acct_id} = '' then a.rplf_no is not null else a.acct_id like $P{acct_id} end)
) a
where a.rplf_no not in (select rplf_no from rf_ewt_remittance)
order by a.client]]>
