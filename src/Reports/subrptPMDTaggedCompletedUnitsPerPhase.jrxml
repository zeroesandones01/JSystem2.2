	</parameter>
	<queryString>
		<![CDATA[select distinct on (y.phase_summary) * from(
		Select E.phase as phase_summary, count (E.lot)::int as phase_count from
		(select b.pbl_id, b.unitstatus_id, a.description, b.status_date, a.phase, a.block, a.lot, to_char(b.status_date,'MM-dd-YYYY') as completion_date
		from mf_unit_info a
		left join rf_unit_status b on a.pbl_id = b.pbl_id
		where (a.phase = $P{phase_no} or $P{phase_no} = '')
		and b.unitstatus_id = '02' and b.status_date::date between $P{date_fr}::date and $P{date_to}::date
AND b.status_id = 'A'
		order by a.phase, a.block::int, a.lot::int, b.status_date)E
		group by E.phase)y]]>
	</queryString>
