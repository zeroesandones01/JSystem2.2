	<parameter name="rplf_no" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT a.invoice_no, coalesce(a.rplf_no,'') as rplf_no, a.policy_no,
get_project_name(a.proj_id) as proj_name, e.company_name as company_name,
a.renewal_batch as batch_no,
c.description as unit_desc, get_client_name(a.entity_id) as client_name, UPPER(FORMAT('%s - %s', TRIM(f.model_code), TRIM(f.firewall_type))) as model_desc,
get_payment_stage(a.entity_id, a.proj_id, a.pbl_id, a.seq_no) as pmt_stage, a.amt_insured,
a.date_from, a.date_to, a.fire_lightning, a.full_earthquake, a.typhoon, a.flood, a.ext_cover, a.rsmd,
(a.fire_lightning + a.full_earthquake + a.typhoon + a.flood + a.ext_cover + a.rsmd) as premium_sub_total,
a.doc_stamps, a.evat, a.fire_srvc_tax, a.loc_govt_tax, a.premium as premium
FROM rf_fire_header a
--LEFT JOIN rf_sold_unit b on b.entity_id = a.entity_id and b.projcode = a.proj_id and b.pbl_id = a.pbl_id and b.seq_no = a.seq_no
LEFT JOIN mf_unit_info c on c.proj_id = a.proj_id and c.pbl_id = a.pbl_id
LEFT JOIN mf_project d on d.proj_id = a.proj_id
LEFT JOIN mf_company e on e.co_id = d.co_id
LEFT JOIN mf_product_model f on f.model_id = c.model_id and coalesce(f.server_id, '') = coalesce(a.server_id, '') and coalesce(f.proj_server, '') = coalesce(a.proj_server, '')
WHERE a.invoice_no IS NOT NULL
AND a.invoice_no = $P{invoice_no}
AND a.rplf_no = $P{rplf_no}
AND a.date_approved IS NOT NULL
--AND a.auto_terminate_date IS NULL
AND a.status_id = 'A';]]>
	</queryString>
