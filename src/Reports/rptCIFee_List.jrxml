	<queryString>
		<![CDATA[select get_client_name(a.entity_id), -1*b.trans_amt as amoutn_paid, a.ar_no,
c.issued_date, a.datecreated, a.batch_no, a.rplf_no, e.entity_name

from rf_ci_fee_payment a
left join (select * from rf_crb_detail where acct_id = '03-02-21-000' and status_id != 'I' )  b on a.ar_no = b.rb_id
left join (select * from rf_crb_header where rb_id in (select rb_id from rf_crb_detail where acct_id = '03-02-21-000' and status_id != 'I' )) c on b.rb_id = c.rb_id and b.doc_id = c.doc_id
left join em_employee d on a.createdby = d.emp_code
left join rf_entity e on d.entity_id = e.entity_id

where a.batch_no = $P{batch_no}::int]]>
	</queryString>
