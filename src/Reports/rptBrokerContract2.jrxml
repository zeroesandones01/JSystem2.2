	<detail>
		<band height="847" splitType="Stretch">
			<textField>
				<reportElement x="0" y="0" width="556" height="19" uuid="c84251f4-727b-4f1e-8eb0-88f2e52146eb"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="Arial" size="14" isBold="true" isUnderline="true"/>
				</textElement>
				<textFieldExpression><![CDATA["BROKERS ACCREDITATION CONTRACT"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="0" y="32" width="540" height="12" uuid="75ba100b-a682-46eb-8d7a-6227b85eacdf"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["KNOW ALL MEN BY THESE PRESENTS:"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="247" y="120" width="40" height="12" uuid="c3762882-a58b-4f65-995e-e43be5d0db47"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["- and -"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="205" y="200" width="138" height="12" uuid="e46a7deb-708f-4f3b-80b4-0bdd4da85aba"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="Arial" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["- WITNESSSETH THAT -"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="9" y="330" width="14" height="12" uuid="cdbedc0d-26c8-4166-a928-aaa4a787b8b9"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["1."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="MMMMM dd, yyyy">
				<reportElement positionType="Float" x="23" y="330" width="517" height="23" uuid="f49447d0-945e-48d3-b6e0-674deb0cdf03"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["The SECOND PARTY understands and agrees that this Contract is non-transferrable and non-assignable to parties outside of this Contract.  This Contract shall be effective <b><u>     " + $P{accred_date_from} + "     </u></b>"+ "and valid until <b><u>      " + $P{accred_date_to}+ "       </u></b>."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="0" y="53" width="541" height="12" uuid="0887453e-6881-4cb6-88da-761091d70f97"/>
				<textElement textAlignment="Justified" verticalAlignment="Top">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["This CONTRACT made and executed in Mandaluyong City, Philippines, by and between:"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="1" y="73" width="539" height="32" uuid="aef1aa80-bc38-4994-b91d-96b2c4e5903c"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["<b><u>       " + $P{comp} + "       </u></b> Development Corporation, a corporation duly organized and existing under the laws of the Philippines, with business and postal address at <b><u>       " + $P{comp_address} + "       </u></b>herein represented by <b><u>       " + $P{co_representative} + "        </u></b>, hereinafter referred to as the FIRST PARTY."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="1" y="142" width="539" height="35" uuid="5f040ecc-0493-41c8-a09a-52a7e46ebdf6"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["<b><u>       " + $V{real_broker} + "       </u></b>, of legal age, licensed Real Estate Broker, Filipino citizen, representing <b><u>     " + $V{real_brokerage} + "      </u></b> , a corporation duly organized and existing under the laws of the Philippines with address at <b><u>     " + $P{agent_address} + "      </u></b> , hereinafter referred to as the SECOND PARTY."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="0" y="223" width="540" height="35" uuid="569add06-06f2-471a-b95e-533494acec7d"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["WHEREAS, the FIRST PARTY is engaged in the development of Subdivision Projects and had appointed and accredited the SECOND PARTY as a broker to Offer for Sale to interested buyers and all unsold and available Subdivision lots and house and lots of the FIRST PARTY and its assigns."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="0" y="268" width="540" height="25" uuid="a6d03c5e-c47e-48ba-bd79-9a25c7034e30"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["WHEREAS, the SECOND PARTY accepted the appointment and agress to abide, observe and perform all the provisions of this Contract including its Annexes."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="0" y="304" width="540" height="15" uuid="ba7e49c7-463f-4d1f-9924-37205bdc2d0b"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["NOW, THEREFORE, for and in consideration of the mutual covenants set forth herein, the parties hereto agree, as follows;"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="9" y="363" width="14" height="12" uuid="78d0b7f8-c16b-45b2-88fc-59d99b773dd6"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["2."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="23" y="363" width="517" height="48" uuid="fae90168-281b-432c-b283-716ab3c6994b"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["This Contract is non-exclusive and shall not preclude the FIRST PARTY from entertaining or entering into any discussion on any offer to purchase a property in any of its Projects from other brokers or direct buyers.  Accordingly, any sale not facilitated through the efforts of the SECOND PARTY shall not entitle him to receive any commission as stipulated under Section 4 of this Contract."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="9" y="418" width="14" height="12" uuid="9c26131b-b432-4c0d-80ea-d6807c4bd038"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["3."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="23" y="418" width="517" height="47" uuid="2249c3c9-ae06-4520-a12b-5f369e9e12d6"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["The SECOND PARTY represents and warrants to the FIRST PARTY that he has obtained, and will maintain such licenses and permits authorizing him to legally act as a broker for the sale and/or offer for sale to the public the FIRST PARTY's subdivision projects and shall comply with all laws, rules and regulations in relation to his conduct and activities as broker."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="22" y="499" width="14" height="12" uuid="b015276b-571a-4cb8-8866-533dd21c5105"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["a."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="35" y="499" width="505" height="60" uuid="92b0afea-1143-4a59-a2b6-1a17342dd42c"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["SECOND PARTY should duly inform his respective prospective buyers of the terms and conditions of the sale as contained in the General Operations Policies and Guidelines for a particular project, the Deed of Restrictions, Contract to Sell and other policies that may be relevant to the purchase of the property. In this connection, the SECOND PARTY shall be responsible for obtaining updates and other information from FIRST PARTY as well as new developments on the projects, particularly changes in available inventory and prices, if any."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="9" y="471" width="14" height="12" uuid="153edc05-70dc-4f99-ba7f-ac6021a86062"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["4."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="23" y="471" width="517" height="23" uuid="4eec54ef-6adc-456f-a2f5-ccadd4c61709"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["The SECOND PARTY undertakes to faithfully perform the following duties and responsibilities expected of him as an Accredited Broker; "]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="22" y="560" width="14" height="12" uuid="a1330d1d-bc03-41da-94c0-d134bec8ed5a"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["b."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="35" y="560" width="505" height="105" uuid="fc043c86-c05c-407b-8e63-a6f190fe45d7"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["SECOND PARTY should be responsible for providing and maintaining an organization to carry out its activities according to its prudent appreciation and its exclusive risk. Without limiting the generalities of the foregoing, the SECOND PARTY shll undertake to FIRST PARTY to (i) obtain and maintain the necessary permits and licenses to operate as required by law (ii) respond to queries (iii) undertakeany necessary effort for an effective promotion for the sale of the property (iv) initiate negotiations for the purchase by customers of the property (v) provide assistance and guidance to the buyer in completing the documentary requirements of his purchase including but not limited to signing of all Contracts and legal documents, collection of all the documents required by the institution which shall finance the purchase, remind buyers of any payment due and other similar act with the intention of rendering full service to the buyer."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="22" y="666" width="14" height="12" uuid="4757cde8-f749-4e78-ae1a-e4ee8ad71a61"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["c."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="35" y="666" width="505" height="60" uuid="8a0a99ca-f1a2-4c22-a4ed-69b0af746d83"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["The SECOND PARTY must ensure clear understanding of the specific directive and instructions which FIRST PARTY may give from time to time with respect to information on the project and the sale, distribution and marketing thereof. The SECOND PARTY shall use advertising and other material authorized and provided by FIRST PARTY for the purpose it was given and to train its employees, staff, sales personnel and consultants, if any, with respect to the utilization of the information contained therein."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="22" y="727" width="14" height="12" uuid="04b7fcf1-f6e0-47c1-a180-c18124373ec6"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["d."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="35" y="727" width="505" height="46" uuid="18eb092b-1d04-4e03-bd10-f8b7fe6585b0"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["It shall be the duty and responsibility of the SECOND PARTY to ensure that payments on the sale of the property facilitated by said him are made to the FIRST PARTY not later than on their actual due dates. The SECOND PARTY should also ensure the timely payment of VAT (if applicable), association dues, real property tax and other related fees and expenses."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="35" y="775" width="505" height="46" uuid="89168d1c-fc1b-42bd-be05-91c55adf3778"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["SECOND PARTY is responsible to obtain and continue to maintain, at his expense, the permits, licenses and any other administrative authorization necessary under applicable laws.  FIRST PARTY shall be held free and harmless from any expenses incurred and any damages sufferred, including the amount paid as a consequence of a decision of any regulatory or judicial authority, deriving from the breach by the SECOND PARTY."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="22" y="775" width="14" height="12" uuid="34f50cee-f927-43bc-90ed-d01f95fcbd48"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["e."]]></textFieldExpression>
			</textField>
		</band>
		<band height="836">
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="9" y="10" width="14" height="12" uuid="f5079eee-8629-45b8-ae8c-89322a3af4f9"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["5."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="23" y="10" width="517" height="58" uuid="47f87923-9dba-4a9c-bad6-d21810208abd"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["The SECOND PARTY is entitled to receive a commission only from the net sales generated by him and accepted by the FIRST PARTY. Net sales shall be defined as attainment of gross monthly sales production less discount, EVAT and cancellation. The sales commission is based on total net selling price as officially reserved net of discounts, miscellaneous and EVAT. The rate of commission and the terms of release thereof shall be based on the prevailing Commission rate and release schedule applicable to the Project sold."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="23" y="76" width="517" height="37" uuid="25728e54-68bc-4926-933a-213a8345df5e"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["All taxes accruing on the receipt by the SECOND PARTY of the commission shall be for the account of the SECOND PARTY. The SECOND PARTY authorizes the FIRST PARTY to withhold the corresponding creditable withholding tax due on the commission and to remit the same to the Bureau of Internal Revenue or its successor agency."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="9" y="76" width="14" height="12" uuid="01bb9724-70b1-409d-b3b2-4e5717b8786e"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["6."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="23" y="121" width="517" height="93" uuid="a235fd11-9a2b-469a-b1ce-d0ae90eba354"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["It is further agreed that no authority has been conferred upon the SECOND PARTY to hire any person on behalf of the FIRST PARTY. Should the SECOND PARTY designate and appoint a third-party entity such as in the case of his real estate salesperson, said third-party shall not be deemed employee of the FIRST PARTY. The SECOND PARTY shall be solely responsible for the acts and omission of his real estate salesperson including the legal requirements to market and sell the Project to the public. The SECOND PARTY shall be solidarily liable to the FIRST PARTY for the performance, acts or omissions of his real estate salesperson and their respective compliance with the terms and conditions of this Contract as well as policies, directive, project guidelines, rules and regulations of the FIRST PARTY in relation to the distribution, promotion and sale of its Projects."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="9" y="121" width="14" height="12" uuid="91878345-b461-4feb-9ac2-55d74d87ab11"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["7."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="23" y="223" width="517" height="106" uuid="30375a5b-7e53-497c-8b3f-ed9d1baa173b"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["Notices, memos, letters or other form of communication maybe sent by the FIRST PARTY to the SECOND PARTY thru his real estate salesperson or employee or thru registered mail to the address stated in this Contract. Further, communications maybe sent to the SECOND PARTY's email address, social network accounts or thru his mobile phone number as indicated in his Accreditation Form. Such notice, memos, letter or other form of communication shall be considered received by the SECOND PARTY (1) upon receipt thereof by his salesperson or employee (2) upon expiration of ten (10) days from date sent thru Registered Mail (3) upon proof of successful message sent out if thru email address, social network accounts or mobile phone number, whichever is earlier.  The FIRST PARTY shall not be held responsible or liable for non-communication to the SECOND PARTY due to wrong and/or not updated personal data."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="9" y="223" width="14" height="12" uuid="48fc6454-e281-4cdd-8711-79261a446fb2"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["8."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="23" y="337" width="517" height="23" uuid="f3f0edef-f868-426b-b88d-a142f6464cd8"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["Neither the SECOND PARTY and/or his real estate salesperson, if any, shall hold themselves out as having any authority whatsoever to bind the FIRST PARTY except as or may otherwise be provided."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="9" y="337" width="14" height="12" uuid="8bb2c410-b6ab-4389-b330-3599ed49a0b5"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["9."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="7" y="369" width="19" height="12" uuid="04bc1966-7230-47d3-8379-fc269029671c"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["10."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="23" y="369" width="517" height="92" uuid="8a89c045-9ae1-4949-832a-2c2824a7678e"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Arial" size="10"/>
				</textElement>
				<textFieldExpression><![CDATA["The SECOND PARTY shall indemnify and hold free and harmless, and shall cause his real estate salesperson, if any, to indemnify and hold free and harmless, the FIRST PARTY and their respective officers and employees, from any and all liabilities and damages, and any and all actions, suits and claims that may be brought or filed against the FIRST PARTY as a result of any misrepresentation or breach by the SECOND PARTY and/or his real estate salesperson, if any, of any or all their warranties and/or representations, or arising from the failure to comply with the FIRST PARTY's policies, or any and all pertinent laws, rules and regulations, including liability for any and all government fines, penalties and surcharges that may be imposed as a result thereof, and any and all expenses that may be incurred by the FIRST PARTY in the defense of such actions, suits or claims."]]></textFieldExpression>
