	<queryString>
		<![CDATA[------CIP Report - Other TYPES
select
a.ntp_no,
trim(a.contract_no) as contract_no,
(trim(b.entity_name)||
	(case when m.entity_name is null then '' else '-' end)||
	coalesce(m.entity_name,'')) as name,
(case when a.vat_rate > 0 then 'VAT' else 'Non-VAT' end) as vatable,
d.accomp_pctg,
d.gross_amt,
(d.gross_amt-d.vat_amt)as net_amt,
(case when a.contract_price is null then a.orig_contract_price else a.contract_price end ) as contract_price,
j.accomp_perc,
g.gr_amt,
(g.gr_amt-h.vt_amt) as nt_amt,
k.proj_alias,

( select count(a.ntp_no) as item_count from (select distinct on (ntp_no) ntp_no from co_ntp_header
    where (CASE WHEN ($P{ntp} = 'All' OR NULLIF(UPPER($P{ntp}), 'NULL') IS NULL) THEN TRUE ELSE a.ntp_type_id ~* $P{ntp} END)

    and (CASE WHEN ($P{proj_id} = 'All' OR NULLIF(UPPER($P{proj_id}), 'NULL') IS NULL) THEN TRUE ELSE a.proj_id ~* $P{proj_id} END)

    and (CASE WHEN ($P{contractor} = 'All' OR NULLIF(UPPER($P{contractor}), 'NULL') IS NULL) THEN TRUE ELSE a.entity_id ~* $P{contractor} END)

    and ntp_no in ( select ntp_no from co_billing_detail where billing_date  <= $P{date_to}  and billing_type ='02' and gross_amt > 0 )
  ) a
) as item_count,

( select sum(case when contract_price is null then orig_contract_price else contract_price end) as tot_contract_price
    from co_ntp_header
    where (CASE WHEN ($P{ntp} = 'All' OR NULLIF(UPPER($P{ntp}), 'NULL') IS NULL) THEN TRUE ELSE a.ntp_type_id ~* $P{ntp} END)

    and (CASE WHEN ($P{proj_id} = 'All' OR NULLIF(UPPER($P{proj_id}), 'NULL') IS NULL) THEN TRUE ELSE a.proj_id ~* $P{proj_id} END)

    and (CASE WHEN ($P{contractor} = 'All' OR NULLIF(UPPER($P{contractor}), 'NULL') IS NULL) THEN TRUE ELSE a.entity_id ~* $P{contractor} END)

    and ntp_no in ( select ntp_no from co_billing_detail where billing_date  <= $P{date_to}  and billing_type ='02' and gross_amt > 0 )
) as tot_contract_price

from ( select * from co_ntp_header where ntp_no in ( select ntp_no from co_billing_detail where billing_date <= $P{date_to} and billing_type ='02' and gross_amt > 0 ) ) a

left join rf_entity b on a.entity_id = b.entity_id

left join mf_project k on a.proj_id = k.proj_id

left join ( select distinct on (ntp_no) ntp_no, max(accomp_pctg)as accomp_perc from co_billing_detail where billing_date <= $P{date_to} group by ntp_no  ) j on a.ntp_no = j.ntp_no

left join ( select ntp_no, sum(gross_amt) as gr_amt from co_billing_detail where billing_type = '02'  and billing_date <= $P{date_to} group by ntp_no ) g on a.ntp_no = g.ntp_no

left join ( select ntp_no, sum(vat_amt) as vt_amt from co_billing_detail where billing_type = '02' and billing_date <= $P{date_to} group by ntp_no ) h on a.ntp_no = h.ntp_no

left join ( select * from co_billing_detail where billing_type = '02' and billing_date <= $P{date_to} ) d on a.ntp_no = d.ntp_no


left join (select distinct on (ntp_no) ntp_no, pbl_id from co_ntp_detail where ntp_no in (select ntp_no from co_ntp_header where co_id = '02' and status_id = 'A' and entity_id = '7115114070'  ) order by ntp_no, pbl_id) jj on a.ntp_no = jj.ntp_no
left join (select distinct on (ntp_no) ntp_no, pbl_id from co_ntp_detail where ntp_no in (select ntp_no from co_ntp_header where co_id = '03' and status_id = 'A') order by ntp_no, pbl_id) kk on jj.pbl_id = kk.pbl_id
left join (select ntp_no, entity_id from co_ntp_header ) l on kk.ntp_no = l.ntp_no
left join rf_entity m on l.entity_id = m.entity_id
left join mf_unit_info n on jj.pbl_id = n.pbl_id

where (case when  $P{phase}= ''  or  $P{phase} = null then true else n.phase = $P{phase}  end) and
(CASE WHEN ($P{ntp} = 'All' OR NULLIF(UPPER($P{ntp}), 'NULL') IS NULL) THEN TRUE ELSE a.ntp_type_id ~* $P{ntp} END)

and (CASE WHEN ($P{proj_id} = 'All' OR NULLIF(UPPER($P{proj_id}), 'NULL') IS NULL) THEN TRUE ELSE a.proj_id ~* $P{proj_id} END)

and (CASE WHEN ($P{contractor} = 'All' OR NULLIF(UPPER($P{contractor}), 'NULL') IS NULL) THEN TRUE ELSE a.entity_id ~* $P{contractor} END)]]>
	</queryString>
