	<queryString>
		<![CDATA[select 'BLK ' || trim(b.block) as block, 'LOT ' || trim(b.lot) as lot, a.work_desc, (select x.hse_color from rf_marketing_scheme_pricelist x where b.proj_id = x.proj_id and b.phase = x.phase
