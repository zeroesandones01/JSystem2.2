	<queryString>
		<![CDATA[select *, now()::date,
coalesce((select x.dateprep::date from rf_client_notices x where x.batch_no = $P{02_BatchNo} limit 1), now()::Date) as date_prep,
$P{05_NoticeType} as type, $P{02_BatchNo} as batch, $P{03_UserName} as user_name
from view_notice_transmittal($P{02_BatchNo}) a
where a.region = $P{06_region}
and a.entity_id in
(
select y.entity_id
from tmp_hdmf x
inner join rf_entity y on x.client_name = y.entity_name
where x.emp_code in
(
$P{emp_code}
)
)
order by name]]>
	</queryString>
