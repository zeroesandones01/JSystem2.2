	<parameter name="asset_filter" class="java.lang.String"/>
	<queryString>
		<![CDATA[select format('%s, %s %s.',c.last_name, c.first_name,left(c.middle_name,1))as current_cust,
format('%s / %s', e.division_alias,f.dept_alias) as div_dept,
h.category_name,
a.asset_code,
a.asset_name,
to_char(a.asset_no,'FM00000000'),
a.item_found,
a.date_scanned,
trim(i.loc_name) as loc_name, a.asset_no::varchar as old_asset
from tbl_asset  a
left join em_employee b on a.current_cust=b.emp_code::int
left join rf_entity c on b.entity_id=c.entity_id
left join rf_location d on b.loc_id=d.loc_id
left join mf_division e on b.div_code=e.division_code
left join mf_department f on b.dept_code=f.dept_code
left join tbl_item g on a.item_id=g.item_id
left join tbl_category h on g.category_id=h.category_id
left join tbl_asset_location i on a.loc_id= i.loc_id
where a.current_cust=$P{emp_code}
and a.status in('ACTIVE','A')
and a.remarks not in('DISPOSED','RETIRED')
and a.item_found in ('true','false')
and (case when $P{item_found}='true' then item_found='true' else item_found in ('true','false') end )
and (case when $P{asset_filter} = 'Fixed Asset' then asset_cost >= 5000
	when $P{asset_filter} = 'Non-fixed Asset' then asset_cost < 5000
   end)
and asset_no not in (select asset_no from tbl_nonfixedasset where status_id = 'Active' and asset_no is not null )


UNION


select format('%s, %s %s.',c.last_name, c.first_name,left(c.middle_name,1))as current_cust,
format('%s / %s', e.division_alias,f.dept_alias) as div_dept,
h.category_name,
null,
a.object_name,
a.object_id as to_char,
a.item_found,
a.date_scanned,
trim(i.loc_name) as loc_name, a.asset_no::varchar as old_asset
from tbl_nonfixedasset  a
left join em_employee b on a.current_cust::int=b.emp_code::int
left join rf_entity c on b.entity_id=c.entity_id
left join rf_location d on b.loc_id=d.loc_id
left join mf_division e on b.div_code=e.division_code
left join mf_department f on b.dept_code=f.dept_code
left join tbl_item g on a.item_id::int=g.item_id
left join tbl_category h on g.category_id=h.category_id
left join tbl_asset_location i on a.loc_id= i.loc_id
where a.current_cust::int=$P{emp_code}
and a.status_id in('Active','A')
and a.remarks not in('DISPOSED','RETIRED')
and a.item_found in ('true','false')
and (case when $P{item_found}='true' then item_found='true' else item_found in ('true','false') end )
and (case when $P{asset_filter} = 'Fixed Asset' then asset_cost >= 5000
	when $P{asset_filter} = 'Non-fixed Asset' then asset_cost >= 1500 and asset_cost < 5000
    end)]]>
	</queryString>
