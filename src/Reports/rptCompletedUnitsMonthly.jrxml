	<queryString>
		<![CDATA[select * from
(select b.pbl_id, to_char(c.status_date,'MONTH yyyy') as month_head, a.description, a.phase, a.block, a.lot, to_char(c.status_date,'MM-dd-YYYY') as completion_date, b.percent_accomplish, b.remarks
	from mf_unit_info a
	left join co_ntp_accomplishment b on a.pbl_id = b.pbl_id
	LEFT JOIN rf_unit_status c on a.pbl_id = c.pbl_id
	where b.percent_accomplish = 100
	and c.unitstatus_id = '02'
	and c.status_date between $P{date_fr}::date and $P{date_to}::date
	and b.status_id = 'A'
	AND c.status_id = 'A'
	order by a.phase, a.block::int, a.lot::int, c.status_date)x

left join

(select distinct on (y.phase_summary)* from(
		Select E.phase as phase_summary, count (E.lot)::int as phase_count from
		(select b.pbl_id, b.unitstatus_id, a.description, b.status_date, a.phase, a.block, a.lot, to_char(b.status_date,'MM-dd-YYYY') as completion_date
		from mf_unit_info a
		left join rf_unit_status b on a.pbl_id = b.pbl_id
		where (case when $P{phase_no} = '' then a.phase in ('1','2','3','4-A', '4') else (a.phase = $P{phase_no} or $P{phase_no} = '')end)
		and b.unitstatus_id = '02' and b.status_date between $P{date_fr}::date and $P{date_to}::date
AND b.status_id = 'A'
		order by a.phase, a.block::int, a.lot::int, b.status_date)E
		group by E.phase)y)y on x.phase = y.phase_summary
where (x.phase= $P{phase_no} or $P{phase_no} = '');]]>
	</queryString>
