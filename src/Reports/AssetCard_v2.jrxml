<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="AssetCard" pageWidth="612" pageHeight="792" whenNoDataType="NoDataSection" columnWidth="572" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="2.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="157"/>
	<parameter name="emp_code" class="java.lang.Integer"/>
	<parameter name="co_logo" class="java.io.InputStream"/>
	<parameter name="company_name" class="java.lang.String" isForPrompting="false"/>
	<parameter name="co_alias" class="java.lang.String"/>
	<queryString>
		<![CDATA[select format('%s, %s %s.',c.last_name, c.first_name,left(c.middle_name,1))as current_cust,
format('%s / %s', e.division_alias,f.dept_alias) as div_dept,
h.category_name,
a.asset_code,
a.asset_name,
to_char(a.asset_no,'FM00000000'),
a.item_found
--a.remarks,
--format('%s',left(a.status,1)) as status
from tbl_asset  a
left join em_employee b on a.current_cust=b.emp_code::int
left join rf_entity c on b.entity_id=c.entity_id
left join rf_location d on b.loc_id=d.loc_id
left join mf_division e on b.div_code=e.division_code
left join mf_department f on b.dept_code=f.dept_code
left join tbl_item g on a.item_id=g.item_id
left join tbl_category h on g.category_id=h.category_id
where a.current_cust=$P{emp_code}
and a.status in('ACTIVE','A')
and a.remarks not in('DISPOSED','RETIRED')
and a.item_found='false'
order by h.category_name,a.asset_no]]>
	</queryString>
