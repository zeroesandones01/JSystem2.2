	<queryString>
		<![CDATA[select a.quantity,
a.description,
a.unit_price,
a.quantity::numeric(19, 4) * a.unit_price::numeric(19, 4) as amount, b.entity_name, (case when terms = null then '' else terms end) as terms ,
c.addr_no || ', ' || c.street || ', ' || c.barangay || ' ' ||d.city_name as address,
f.entity_name as prep_by
from rf_purchase_order a
left join rf_entity b on a.supplier=b.entity_id
left join rf_entity_address c on b.entity_id=c.entity_id
left join mf_city d on c.city_id=d.city_id
left join em_employee e on a.requester = e.emp_code
left join rf_entity f on e.entity_id = f.entity_id
where po_no=$P{po_no}]]>
