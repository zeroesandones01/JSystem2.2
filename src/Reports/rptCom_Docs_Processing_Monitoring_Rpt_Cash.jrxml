	<queryString>
		<![CDATA[select

a.cv_no,
b.amount,
upper(trim(g.entity_name)) as created_by,
upper(trim(h.entity_name)) as last_edited_by,
to_char(a.date_created,'MM-dd-yyyy') as date_created,
to_char(a.date_posted,'MM-dd-yyyy') as date_posted,
to_char(a.date_paid,'MM-dd-yyyy') as date_paid,
trim(e.return_message) as return_message,
upper(trim(f.entity_name)) as payee

from rf_cv_header a
left join (
	select distinct on (cv_no, co_id) a.cv_no, a.co_id, sum(tran_amt) as amount from (
	select cv_no, co_id, tran_amt  from rf_cv_detail where bal_side = 'D' and status_id = 'A' and co_id = $P{co_id}) a
	group by cv_no, co_id  ) b on trim(a.cv_no) = trim(b.cv_no) and a.co_id = b.co_id
left join em_employee c on a.created_by=c.emp_code
left join em_employee d on a.edited_by=d.emp_code
left join (select * from rf_doc_return_message where status_id = 'A' and doc_id = '13' ) e on a.cv_no = e.doc_no and a.co_id=e.co_id
left join rf_entity f on a.entity_id1 = f.entity_id
left join rf_entity g on c.entity_id = g.entity_id
left join rf_entity h on c.entity_id = h.entity_id

where a.status_id = $P{status_id}
and a.remarks NOT LIKE 'Commission%' AND a.remarks NOT LIKE 'Promo/Incentive%'
and a.co_id = $P{co_id}
and a.proc_id = $P{proc_id}
and a.pay_type_id = 'A'
and (case when $P{date_name} = 'cv_date' then a.cv_date else
	case when  $P{date_name} = 'date_posted' then a.date_posted::date
	else a.date_paid ::date end end) >= $P{date_from}
and (case when $P{date_name} = 'cv_date' then a.cv_date else
	case when  $P{date_name} = 'date_posted' then a.date_posted::date
	else a.date_paid::date end end) - interval '1 day' <= $P{date_to}
order by a.cv_no]]>
	</queryString>
