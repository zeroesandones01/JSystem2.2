	<queryString>
		<![CDATA[select

a.rb_id,
a.canceled_date as date_canceled,
coalesce(b.OR_Date, b.Trans_Date) as date_issued,
trim(a.particulars) as particulars,
ltrim(rtrim(e.doc_desc)) as receipt_type,
(case when a.pymnttype = 'A' then 'Cash' else 'Check' end) as pymnt_type,
a.amount,
coalesce(ltrim(rtrim(b.pbl_id)),'none') as pbl_id,
coalesce(c.Description, 'none') as description,
coalesce (b.seq_no, 0) as seq_no,
ltrim(rtrim(d.entity_name)) as client_name

from rf_crb_header a
left join
(
select or_date, trans_date, pbl_id, seq_no, entity_id, pay_Rec_ID, status_id, proj_id, client_seqno, branch_id from rf_payments union all
select a.trans_date, a.booking_date, a.pbl_id, a.seq_no, a.entity_id, a.pay_header_ID, a.status_id, a.proj_id, a.client_seqno, branch_id
	from rf_pay_header a
left join rf_pay_detail b on a.client_seqno = b.client_seqno
where /*b.receipt_type = '03' and*/ b.part_type = '054' union all
select trans_date, actual_date, pbl_id, seq_no, entity_id, tra_header_ID, status_id, proj_id, client_seqno, branch_id from rf_tra_header
) b on a.pay_rec_id::int = b.pay_Rec_ID
and to_char(a.issued_date,'yyyy-MM-dd')= to_char(b.trans_date,'yyyy-MM-dd')
and a.reference_no = b.client_seqno

left join mf_unit_info c  on b.pbl_id = c.Pbl_ID and b.proj_id = c.Proj_id
left join rf_entity d  on b.Entity_ID = d.entity_id
left join mf_system_doc e on a.doc_id = e.doc_id

where a.status_id = 'I'
and ( case when $P{proj_id} = '' or $P{proj_id} is null then a.rb_id is not null else a.proj_id = $P{proj_id}  end )
and ( case when $P{branch_id} = '' or $P{branch_id} is null then a.rb_id is not null else b.branch_id = $P{branch_id}  end )
and a.canceled_date >= $P{date_from}
and a.canceled_date - interval '1 day' < $P{date_to}]]>
	</queryString>
