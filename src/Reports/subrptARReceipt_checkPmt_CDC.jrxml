	<queryString>
		<![CDATA[SELECT b.receipt_no, a.entity_id, a.proj_id, a.pbl_id, a.trans_date, a.total_amt_paid, b.part_type, SUM(b.amount) as amount, b.bank, b.branch, b.acct_no, b.check_no, b.check_date
