(select emp_pos from em_employee where emp_code = $P{emp_code}) as position,
(select emp_rank from em_employee where emp_code = $P{emp_code}) as rank,
(select dept_alias from mf_department
	where dept_code in (select dept_code from em_employee where  emp_code = $P{emp_code})) as dept

from mf_privileges

where emp_code =  $P{emp_code}

order by module, privileges]]>
