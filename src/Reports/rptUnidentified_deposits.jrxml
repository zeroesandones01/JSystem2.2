	<queryString>
		<![CDATA[select

a.record_id,
a.amount,
a.bank_acct_id,
a.bank_account,
to_char(a.deposit_date,'MM-dd-yyyy') as deposit_date,
upper(trim(ee.last_name))||', '||left(upper(trim(ee.first_name)),1)||left(upper(trim(ee.middle_name)),1) as tagged_by,
to_char(a.tagged_date,'MM-dd-yyyy') as tagged_date,
upper(trim(ff.last_name))||', '||left(upper(trim(ff.first_name)),1)||left(upper(trim(ff.middle_name)),1) as edited_by,
to_char(a.edited_date,'MM-dd-yyyy') as edited_date

from tf_unidentified_dep a
left join em_employee e on a.tagged_by = e.emp_code
left join rf_entity ee on e.entity_id = ee.entity_id
left join em_employee f on a.edited_by = f.emp_code
left join rf_entity ff on f.entity_id = ff.entity_id

where a.status_id = 'A'
and a.co_id = $P{co_id}
order by a.deposit_date, a.record_id]]>
	</queryString>
