	<queryString>
		<![CDATA[select a.phase, sum(a.amount) as amount
from
(
select split_part(a.c_unit, ' ', 2)::varchar as phase, a.c_amount as amount
from view_hdmf_collection($P{03_CoID}, $P{04_ProID}, $P{11_Phase}, $P{09_dteFrom}, $P{10_dteTo}) a
where a.c_entity_name in (select z.client_name from tmp_hdmf z where z.emp_code = (select x.emp_code from em_employee x inner join rf_entity y on x.entity_id = y.entity_id where y.entity_name = $P{07_User}))
) a
group by a.phase
order by NULLIF(regexp_replace(a.phase, '\D','','g'), '')::numeric]]>
	</queryString>
