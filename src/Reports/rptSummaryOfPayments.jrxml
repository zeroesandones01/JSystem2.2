	<queryString>
		<![CDATA[SELECT a.pymnt_type, a.trans_date, b.partdesc, (CASE WHEN a.or_doc_id IS NOT NULL THEN a.or_no END) as or_no,
(CASE WHEN a.or_doc_id IS NULL THEN a.or_no ELSE a.ar_no END) as ar_no,
(CASE WHEN a.si_no IS NOT NULL THEN a.si_no END) as si_no,

(CASE WHEN a.pymnt_type = 'A' THEN NULL ELSE FORMAT('%s - %s', c.bank_alias, d.bank_branch_location) END) as bank_branch, a.check_no, a.check_date, coalesce(g.checkstat_desc, e.checkstat_desc) as checkstat_desc,
a.amount, a.remarks, (SELECT tran_date FROM rf_buyer_status WHERE entity_id = $P{entity_id} AND proj_id = $P{proj_id} AND pbl_id = $P{pbl_id} AND seq_no = $P{seq_no} AND byrstatus_id = '17' AND status_id = 'A') as reservation_date, (SELECT b.status_desc FROM rf_buyer_status a LEFT JOIN mf_buyer_status b ON b.byrstatus_id = a.byrstatus_id WHERE a.entity_id = $P{entity_id} AND a.proj_id = $P{proj_id} AND a.pbl_id = $P{pbl_id} AND a.seq_no = $P{seq_no} AND a.status_id = 'A' ORDER BY tran_date DESC LIMIT 1) as current_status,
(SELECT tran_date FROM rf_buyer_status WHERE entity_id = $P{entity_id} AND proj_id = $P{proj_id} AND pbl_id = $P{pbl_id} AND seq_no = $P{seq_no} AND byrstatus_id = '18' AND status_id = 'A') as date_completion
FROM rf_payments a
LEFT JOIN mf_pay_particular b ON b.pay_part_id = a.pay_part_id
LEFT JOIN mf_bank c ON c.bank_id = a.bank_id
LEFT JOIN mf_bank_branch d ON d.bank_id = a.bank_id AND d.bank_branch_id = a.bank_branch_id
LEFT JOIN mf_check_status e ON e.checkstat_id = a.check_stat_id
left join rf_payments f on (a.receipt_id = f.or_no or a.receipt_id = f.ar_no) and a.check_no = f.check_no and a.amount = f.amount
LEFT JOIN mf_check_status g ON f.check_stat_id = g.checkstat_id
WHERE a.entity_id = $P{entity_id} AND a.proj_id = $P{proj_id} AND a.pbl_id = $P{pbl_id}
AND a.seq_no = $P{seq_no}
and a.status_id NOT IN ('X')
--AND a.status_id IN ('A')
ORDER BY a.actual_date::DATE, a.check_date;]]>
	</queryString>
