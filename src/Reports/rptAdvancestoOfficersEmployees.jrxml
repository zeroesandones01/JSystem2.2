	<queryString>
		<![CDATA[select * from (

select

(case when date_paid is null then 'NOT YET RELEASED' else 'PAID OUT' end) as type,
upper(trim(division_name)) as div,
a.rplf_no,
c.rplf_date,
d.entity_name,
a.pv_amt,
f.cv_no,
f.date_paid,
b.acct_name,
a.part_desc

from ( select * from rf_request_detail where rplf_no in ( select rplf_no from rf_request_header where rplf_type_id in ('02','07') ) and status_id != 'I' ) a
left join mf_boi_chart_of_accounts b on a.acct_id = b.acct_id
left join (select * from rf_request_header where status_id != 'I') c on a.rplf_no = c.rplf_no
left join rf_entity d on c.entity_id2 = d.entity_id
left join (select * from rf_pv_header where status_id != 'I') e on a.rplf_no = e.pv_no
left join (select * from rf_cv_header where status_id != 'I') f on e.cv_no = f.cv_no
left join em_employee g on c.entity_id2 = g.entity_id
left join (select * from mf_division where status_id != 'I') h on g.div_code = h.division_code

where a.rplf_no not in (select distinct on (rplf_no) rplf_no from rf_liq_header where status_id not in ('I', 'X'))
and (case when $P{proj_id} = '' or $P{proj_id} is null then a.rplf_no is not null else a.project_id = $P{proj_id} end)
and (case when $P{div_id} = '' or $P{div_id} is null then a.rplf_no is not null else g.div_code = $P{div_id} end)
and (case when $P{emp_id} = '' or $P{emp_id} is null then a.rplf_no is not null else c.entity_id1 = $P{emp_id} end)
and c.rplf_date <= $P{date_to}

order by acct_name
) a

order by a.type desc, a.div, a.entity_name, a.rplf_no]]>
	</queryString>
