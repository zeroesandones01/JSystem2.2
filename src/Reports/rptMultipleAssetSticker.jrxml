	<queryString>
		<![CDATA[select * from
(
select *, ROW_NUMBER () OVER  ( order by
	--a.asset_no::varchar,
	a.asset_code_1) as marker
from
(
	select  ROW_NUMBER() OVER ( order BY
	a.asset_code ,
	a.asset_no::varchar,
	a.asset_name,
	to_char(a.asset_no, 'FM00000000'),
	c.entity_name,
	(case when d.company_name is null then 'ACERHOMES DEVELOPMENT CORPORATION' else d.company_name end),
	lower(format('%s%s%s',left(c.first_name,1),left(c.middle_name,1),c.last_name)),a.date_acquired) AS "ROW",
	a.asset_code as asset_code_1,
	a.asset_no::varchar as asset_no_1,
	a.asset_name as asset_name_1,
	to_char(a.asset_no, 'FM00000000') as anum_1,
	c.entity_name as entity_name_1,
	(case when d.company_name is null then 'ACERHOMES DEVELOPMENT CORPORATION' else d.company_name end) as company_name_1,
	lower(format('%s%s%s',left(c.first_name,1),left(c.middle_name,1),c.last_name)) as cuser_1,a.date_acquired as date_acquired_1,coalesce(remarks2,'') as remarks2_1
	from tbl_asset  a
	left join em_employee b on a.current_cust::int=b.emp_code::int
	left join rf_entity as c on b.entity_id=c.entity_id
	left join mf_company d on b.co_id=d.co_id
	where a.asset_no::int in (select asset_no::int from tmp_asset where emp_code =$P{emp_code} )
) a where "ROW" % 2 = 1
)  a
left join
(
select *, ROW_NUMBER () OVER  ( order by
	--a.asset_no::varchar,
	a.asset_code_2) as marker
from
(select  ROW_NUMBER() OVER ( order BY
	a.asset_code,
	a.asset_no::varchar,
	a.asset_name,
	to_char(a.asset_no, 'FM00000000'),
	c.entity_name,
	(case when d.company_name is null then 'ACERHOMES DEVELOPMENT CORPORATION' else d.company_name end),
	lower(format('%s%s%s',left(c.first_name,1),left(c.middle_name,1),c.last_name)),a.date_acquired) AS "ROW",
	a.asset_code as asset_code_2,
	a.asset_no::varchar as asset_no_2,
	a.asset_name as asset_name_2,
	to_char(a.asset_no, 'FM00000000') as anum_2,
	c.entity_name as entity_name_2,
	(case when d.company_name is null then 'ACERHOMES DEVELOPMENT CORPORATION' else d.company_name end) as company_name_2,
	lower(format('%s%s%s',left(c.first_name,1),left(c.middle_name,1),c.last_name)) as cuser_2,a.date_acquired as date_acquired_2,coalesce(remarks2,'') as remarks2_2
	from tbl_asset  a
	left join em_employee b on a.current_cust::int=b.emp_code::int
	left join rf_entity as c on b.entity_id=c.entity_id
	left join mf_company d on b.co_id=d.co_id
	where a.asset_no::int in (select asset_no::int from tmp_asset where emp_code = $P{emp_code})) a where "ROW" % 2 = 0
)  b
on a.marker=b.marker]]>
