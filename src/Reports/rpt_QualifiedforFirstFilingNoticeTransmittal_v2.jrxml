	<queryString>
		<![CDATA[select *,
coalesce((select x.dateprep::date from rf_client_notices x where x.batch_no = $P{02_BatchNo} limit 1), now()::Date) as date_prep
from view_notice_transmittal($P{02_BatchNo}) a
where a.entity_id in
(
select y.entity_id
from tmp_hdmf x
inner join rf_entity y on x.client_name = y.entity_name
where x.emp_code in
(
	SELECT z.emp_code
	FROM em_employee z
	INNER JOIN rf_entity x1 ON z.entity_id = x1.entity_id
	WHERE x1.entity_name = $P{03_UserName}
)
)
order by a.name]]>
