	<queryString>
		<![CDATA[SELECT

FORMAT('%-*s', 70, FORMAT('%*s', (35  + char_length(FORMAT('%s %s', TRIM(c.first_name), TRIM(c.last_name)))) - (char_length(FORMAT('%s %s', TRIM(c.first_name), TRIM(c.last_name))) / 2), FORMAT('%s %s', TRIM(c.first_name), TRIM(c.last_name)))) as client_name,

d.proj_name, b.phase, b.block, b.lot, b.lotarea, b.floorarea, e.model_id, e.model_desc, e.model_class, e.with_color_scheme, f.company_name, f.company_alias, array_length(b.color_scheme_id) as color_scheme_count, h.hse_color

FROM rf_sold_unit a
LEFT JOIN mf_unit_info b ON b.proj_id = a.projcode and b.pbl_id = a.pbl_id
LEFT JOIN rf_entity c ON c.entity_id = a.entity_id
LEFT JOIN mf_project d ON d.proj_id = a.projcode
LEFT JOIN mf_product_model e ON e.model_id = a.model_id
LEFT JOIN mf_company f ON f.co_id = d.co_id
LEFT JOIN mf_product_model g ON g.model_id = a.model_id
LEFT JOIN rf_marketing_scheme_pricelist h on h.proj_id = a.projcode and h.phase = b.phase and h.block = b.block and h.lot = b.lot

WHERE a.entity_id = $P{entity_id}
AND a.projcode = $P{proj_id}
AND a.pbl_id = $P{pbl_id}
AND a.seq_no = $P{seq_no};]]>
