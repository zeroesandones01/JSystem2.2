	<queryString>
		<![CDATA[SELECT a.rec_id, get_project_name(a.proj_id) as proj_name ,
TRIM(b.phase) as phase, TRIM(b.block) as block, TRIM(b.lot) as lot,
get_client_name(a.entity_id) as client_name, get_nsp(a.entity_id, a.pbl_id, a.seq_no ,a.proj_id) as nsp,
get_model_name(c.model_id) as model_desc, a.amt_insured as insurance_amt,
age(a.date_to, a.date_from)::VARCHAR as term, a.date_from, a.date_to, get_client_name(a.insurance_comp) as insurance_company, a.policy_no,
get_ob(a.entity_id, a.proj_id, a.pbl_id, a.seq_no) as ob
FROM rf_fire_header a
LEFT JOIN mf_unit_info b on b.proj_id = a.proj_id and b.pbl_id = a.pbl_id
LEFT JOIN rf_sold_unit c on c.entity_id = a.entity_id and c.projcode = a.proj_id and c.pbl_id = a.pbl_id and c.seq_no = a.seq_no
WHERE a.date_approved IS NOT NULL
AND a.proj_id = $P{proj_id}
AND a.date_terminated IS NULL
AND a.auto_terminate_date IS NULL
ORDER BY a.rec_id;]]>
	</queryString>
