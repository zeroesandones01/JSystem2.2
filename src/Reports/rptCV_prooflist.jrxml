	<parameter name="co_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select

a.cv_no,
to_char(a.cv_date, 'MM-dd-yyyy') as cv_date,
to_char(a.date_paid, 'MM-dd-yyyy') as date_paid,
(case when a.pay_type_id = 'A' then 'Cash' else 'Check' end) as pmt_type,
e.bank_alias,
c.bank_acct_no,
b.check_no,
--trim(f.entity_name) as payee,
(SELECT get_client_name(entity_id) from rf_check where cv_no = a.cv_no and co_id = a.co_id and status_id = 'A') as payee,
trim(g.entity_name) as availer,
h.cv_amt,
(case when a.status_id = 'D' then 'Deleted' else
	case when proc_id = 0 then 'For Checking' else
	case when proc_id = 1 then 'For Approval' else
	case when proc_id = 2 then 'For Check Signature' else
	case when proc_id = 3 then 'For Release to Payee' else
	case when proc_id = 5 then 'Paid Out' else '' end end end end end end ) as status_name

from rf_cv_header a
left join (select * from rf_check where status_id != 'I') b on a.cv_no = b.cv_no and a.co_id = b.co_id
left join mf_bank_account c on b.bank_acct_id = c.bank_acct_id
left join mf_bank_branch d on c.bank_branch_id = d.bank_branch_id
left join mf_bank e on d.bank_id = e.bank_id
left join rf_entity f on a.entity_id1 = f.entity_id
left join rf_entity g on a.entity_id2 = g.entity_id
left join ( select distinct on (cv_no) cv_no, co_id, sum(tran_amt) as cv_amt from rf_cv_detail where status_id != 'I' and bal_side = 'D' group by cv_no, co_id ) h on a.cv_no = h.cv_no
	and a.co_id = h.co_id
left join mf_record_status i on a.status_id = i.status_id

where a.cv_no != ''
and a.cv_date::date >= $P{date_from}
and a.cv_date::date <= $P{date_to}
and a.co_id = $P{co_id}
and ( case when $P{bank_acct_id} = '' or $P{bank_acct_id} is null then a.cv_no is not null else b.bank_acct_id = $P{bank_acct_id}  end )
and ( case when $P{payee_id} = '' or $P{payee_id} is null then a.cv_no is not null else a.entity_id1 = $P{payee_id}  end )
and ( case when $P{availer_id} = '' or $P{availer_id} is null then a.cv_no is not null else a.entity_id2 = $P{availer_id}  end )
and ( case when $P{prepared_id} = '' or $P{prepared_id} is null then a.cv_no is not null else a.created_by = $P{prepared_id}  end )
and ( case when $P{cv_from} = 0 or $P{cv_from} is null then a.cv_no is not null else a.cv_no::int >= $P{cv_from}   end )
and ( case when $P{cv_to} = 0 or $P{cv_to} is null then a.cv_no is not null else a.cv_no::int <= $P{cv_to}  end )
and ( case when $P{pmt_type_id} = '' then a.cv_no is not null else a.pay_type_id = $P{pmt_type_id} end )
and ( case when $P{status_id} = '' then a.cv_no is not null else a.status_id = $P{status_id} end )
and ( case when $P{proc_id} = '' then a.cv_no is not null else a.proc_id::text = $P{proc_id} end )

order by a.cv_no]]>
