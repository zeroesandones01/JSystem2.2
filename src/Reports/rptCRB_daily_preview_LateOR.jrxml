	<queryString>
		<![CDATA[select A.rb_id, trim(C.doc_alias) as doc_alias, (trim(D.phase)||'-'||trim(D.block)||'-'||trim(D.lot)) as pbl,
trim(E.entity_name) as client, trim(A.acct_id) as acct_id, trim(F.acct_name) as acct_name,
( case when A.trans_amt > 0 then A.trans_amt else 0.00 end) as debit,
( case when A.trans_amt < 0 then -1*A.trans_amt else 0.00 end) as credit,
trim(A.remarks) as remarks, A.co_id, trim(H.status_desc) as status
from (select * from rf_crb_detail where status_id != 'I') A
join
(

select x.*
from rf_crb_header x
inner join
(select * from rf_payments y where y.remarks ~* 'Late LTS/BOI' or y.remarks ~* 'Late OR Issuance for Good Check') y
on y.client_seqno = x.reference_no and coalesce(y.si_date, y.or_date)::date is not null
and x.pay_rec_id::int = y.pay_rec_id::int and COALESCE(y.si_no, y.or_no) = x.rb_id
where coalesce(y.si_date,y.or_date)::Date = $P{cdr_date}::date
and (x.co_id = $P{co_id} or $P{co_id} = 'All')

) B on A.rb_id = B.rb_id  and A.co_id=B.co_id  and A.doc_id = B.doc_id and A.pay_rec_id = B.pay_rec_id  -- distinct on (rb_id)
left join mf_doc_details C on A.doc_id = C.doc_id
left join mf_unit_info  D on A.proj_id = D.proj_id and A.pbl_id = D.pbl_id
left join rf_entity E on A.entity_id = E.entity_id
left join mf_boi_chart_of_accounts F on A.acct_id = F.acct_id
left join
(
	select distinct on (or_no, or_doc_id, pr_doc_id) or_no,
	(case when or_doc_id is null then pr_doc_id  else or_doc_id end),
	trans_date, pay_rec_id from rf_payments
	union all
	select distinct on (receipt_no, receipt_type) receipt_no, receipt_type, tran_date, header_id
	from rf_tra_detail
) G on B.rb_id = G.or_no and B.pay_rec_id::int = G.pay_rec_id
left join mf_record_status H on B.status_id = H.status_id
left join
(
	select branch_id, coalesce(si_no,or_no) as or_no, pay_rec_id from rf_payments union all
	select branch_id, receipt_no, tra_header_id from rf_tra_header
) I on A.rb_id = I.or_no and A.pay_rec_id::int = I.pay_rec_id
where A.trans_amt <> 0
and (case when $P{branch} = 'All'  or $P{branch} is null then A.rb_id is not null else I.branch_id = $P{branch} end)
and (case when $P{proj_id} = 'All' or $P{proj_id} is null then A.rb_id is not null else B.proj_id = $P{proj_id} end)
and
A.rb_id in
(
	select coalesce(x.si_no,x.or_no)
	from rf_payments x
	where x.status_id != 'I'
	and (coalesce(remarks, '') ~* 'Late LTS/BOI' OR coalesce(remarks, '') ~* 'Late OR Issuance for Good Check')
	and coalesce(coalesce(x.si_date,x.or_date)::char(10), '') <> ''
) and A.trans_amt <> 0
order by A.rb_id, A.co_id, A.doc_id, A.trans_amt desc]]>
	</queryString>
