<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="rptContractToSell" pageWidth="612" pageHeight="936" columnWidth="512" leftMargin="50" rightMargin="50" topMargin="20" bottomMargin="20">
