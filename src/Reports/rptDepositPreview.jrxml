		<![CDATA[select

b.bank_alias,
c.bank_branch_location,
a.check_no,
a.tran_amt,e.proj_alias,
f.phase,g.particulars

from cs_dp_chk_detail a
left join mf_bank b on a.bank_id = b.bank_id
left join mf_bank_branch c on a.bank_branch_id = c.bank_branch_id
left join rf_payments d on  a.pay_rec_id = d.pay_rec_id
left join mf_project e on d.proj_id = e.proj_id
left join mf_unit_info f on d.proj_id = f.proj_id and d.pbl_id = f.pbl_id and d.server_id = f.server_id and d.proj_server = f.proj_server
left join mf_pay_particular g on d.pay_part_id = g.pay_part_id

where a.dep_no = $P{deposit_no}
and a.status_id = 'A']]>
	</queryString>
