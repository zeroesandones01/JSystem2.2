	<queryString>
		<![CDATA[select

upper(trim(b.entity_name)) as agent_name,
trim(c.dept_name) as dept,
d.position_desc as position,
to_char(a.atm_regdate,'MM-dd-yyyy') as tag_date,
a.atm_no as atm_no,
'Released' as status,
trim(g.entity_name) as released_by

from mf_sellingagent_info a
left join rf_entity b on a.agent_id = b.entity_id
left join mf_department c on a.dept_id = c.dept_code
left join mf_sales_position d on a.salespos_id  = d.position_code
left join mf_record_status e on a.status_id = e.status_id
left join em_employee f on a.atm_rlsd_by = f.emp_code
left join rf_entity g on f.entity_id = g.entity_id

where to_char(a.atm_rlsd_date,'MM/dd/yyyy') = $P{rlsd_date}]]>
	</queryString>
