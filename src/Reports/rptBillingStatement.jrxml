	<queryString>
		<![CDATA[SELECT b.entity_id as client_id, get_client_name(b.entity_id) as client_name, c.datersvd as reservation_date, TRIM(d.description) as unit, TRIM(e.model_desc) as model,
	TRIM(f.proj_name) as project, get_client_address(b.entity_id) as address, TRIM(b.billingno) as bill_no, b.billingduedate as due_date, a.amtdue as amount_due,
	a.is_past_due, TRIM(a.pmtstage) as payment_stage, c.sellingprice, a.bpterm, a.osbal, TRIM(g.first_name) as first_name, TRIM(g.last_name) as last_name,
	a.trandate, TRIM(a.bank) as bank, TRIM(a.acctno) as acctno, TRIM(a.checkno) as checkno, a.lastamt, a.pmttype, a.remaining_pdc, h.company_alias

FROM rf_regular_billing a
LEFT JOIN rf_client_notices b ON b.billingno = a.billno
LEFT JOIN rf_sold_unit c ON c.entity_id = b.entity_id AND c.projcode = b.projcode AND c.pbl_id = b.pbl_id AND c.seq_no = b.seq_no
LEFT JOIN mf_unit_info d ON d.proj_id = b.projcode AND d.pbl_id = b.pbl_id
LEFT JOIN mf_product_model e ON e.model_id = d.model_id
LEFT JOIN mf_project f ON f.proj_id = d.proj_id
LEFT JOIN rf_entity g ON g.entity_id = b.entity_id
LEFT JOIN mf_company h ON h.co_id = f.co_id

WHERE b.batch_no = $P{batch_no}

ORDER BY get_client_name(b.entity_id);]]>
