	<queryString>
		<![CDATA[select

row_number() over(PARTITION BY a.pbl_id, a.seq_no ORDER BY a.pbl_id, a.seq_no) as entry_no,
trim(description) as unit_desc,
trim(b.entity_name) as client_name,
O.issued_date as takeout_date,
trim(j.type_alias_too) as mode_of_pmt,
e.sellingprice as selling_price,
(coalesce(-1*h.prin_paid,0) + coalesce(hh.prin_paid,0))  as prin_payment_2015,
(coalesce(-1*k.prin_paid,0) + coalesce(kk.prin_paid,0))  as prin_payment_2016,
(coalesce(-1*l.prin_paid,0) + coalesce(ll.prin_paid,0))  as prin_payment_2017,
(coalesce(-1*m.prin_paid,0) + coalesce(mm.prin_paid,0))  as prin_payment_2018,
(coalesce(-1*n.prin_paid,0) + coalesce(nn.prin_paid,0))  as prin_payment_2019,
a.pbl_id,
a.seq_no,
(((coalesce(-1*h.prin_paid,0) + coalesce(hh.prin_paid,0)) +
(coalesce(-1*k.prin_paid,0) + coalesce(kk.prin_paid,0)) +
(coalesce(-1*l.prin_paid,0) + coalesce(ll.prin_paid,0)) +
(coalesce(-1*m.prin_paid,0) + coalesce(mm.prin_paid,0)) +
(coalesce(-1*n.prin_paid,0) + coalesce(nn.prin_paid,0)))/e.sellingprice)*100 as perc_completion


from ( select distinct on (proj_id, pbl_id, seq_no) proj_id, pbl_id, seq_no, co_id, sum(trans_amt)as prin_amt, entity_id
	from rf_crb_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and trans_amt < 0
	--and (rb_id, pay_rec_id, doc_id) in (select rb_id, pay_rec_id, doc_id from rf_crb_header where posted_date is not null)
	and case when $P{proj_id} = '' then rb_id is not null else proj_id = $P{proj_id} end
	and case when $P{phase_no} = 'ALL' then rb_id is not null else phase = $P{phase_no} end
	group by proj_id, pbl_id, seq_no, co_id, entity_id ) a

left join rf_entity b on a.entity_id = b.entity_id

left join mf_unit_info c on a.pbl_id = c.pbl_id and a.proj_id = c.proj_id

left join ( select entity_id, projcode, pbl_id, seq_no, doc_id, date_notarized from rf_buyer_documents where doc_id in ('39', '06') and status_id = 'A' )
	d on a.entity_id = d.entity_id and a.proj_id = d.projcode and a.pbl_id = d.pbl_id and a.seq_no = d.seq_no

left join (select * from rf_sold_unit where status_id = 'A') e on a.proj_id = e.projcode and a.pbl_id = e.pbl_id and a.seq_no = e.seq_no

left join (select distinct on (proj_id, pbl_id, seq_no, co_id) proj_id, pbl_id, seq_no, co_id, sum(trans_amt) as prin_paid
	from rf_crb_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and trans_amt < 0 and rb_fiscal_year = '2015'
	group by proj_id, pbl_id, seq_no, co_id) h on a.proj_id = h.proj_id and a.pbl_id = h.pbl_id and a.seq_no = h.seq_no and a.co_id = h.co_id

left join (select distinct on (project_id, pbl_id, seq_no, co_id) project_id, pbl_id, seq_no, co_id, sum(tran_amt) as prin_paid
	from rf_jv_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and bal_side = 'C'
	and (jv_no, co_id) in (select jv_no, co_id from rf_jv_header where fiscal_yr = '2015')
	group by project_id, pbl_id, seq_no, co_id) hh on a.proj_id = hh.project_id and a.pbl_id = hh.pbl_id and a.seq_no = hh.seq_no and a.co_id = hh.co_id

left join (select distinct on (proj_id, pbl_id, seq_no, co_id) proj_id, pbl_id, seq_no, co_id, sum(trans_amt) as prin_paid
	from rf_crb_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and trans_amt < 0 and rb_fiscal_year = '2016'
	group by proj_id, pbl_id, seq_no, co_id) k on a.proj_id = k.proj_id and a.pbl_id = k.pbl_id and a.seq_no = k.seq_no and a.co_id = k.co_id

left join (select distinct on (project_id, pbl_id, seq_no, co_id) project_id, pbl_id, seq_no, co_id, sum(tran_amt) as prin_paid
	from rf_jv_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and bal_side = 'C'
	and (jv_no, co_id) in (select jv_no, co_id from rf_jv_header where fiscal_yr = '2016')
	group by project_id, pbl_id, seq_no, co_id) kk on a.proj_id = kk.project_id and a.pbl_id = kk.pbl_id and a.seq_no = kk.seq_no and a.co_id = kk.co_id

left join (select distinct on (proj_id, pbl_id, seq_no, co_id) proj_id, pbl_id, seq_no, co_id, sum(trans_amt) as prin_paid
	from rf_crb_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and trans_amt < 0 and rb_fiscal_year = '2017'
	group by proj_id, pbl_id, seq_no, co_id) l on a.proj_id = l.proj_id and a.pbl_id = l.pbl_id and a.seq_no = l.seq_no and a.co_id = l.co_id

left join (select distinct on (project_id, pbl_id, seq_no, co_id) project_id, pbl_id, seq_no, co_id, sum(tran_amt) as prin_paid
	from rf_jv_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and bal_side = 'C'
	and (jv_no, co_id) in (select jv_no, co_id from rf_jv_header where fiscal_yr = '2017')
	group by project_id, pbl_id, seq_no, co_id) ll on a.proj_id = ll.project_id and a.pbl_id = ll.pbl_id and a.seq_no = ll.seq_no and a.co_id = ll.co_id

left join (select distinct on (proj_id, pbl_id, seq_no, co_id) proj_id, pbl_id, seq_no, co_id, sum(trans_amt) as prin_paid
	from rf_crb_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and trans_amt < 0 and rb_fiscal_year = '2018'
	group by proj_id, pbl_id, seq_no, co_id) m on a.proj_id = m.proj_id and a.pbl_id = m.pbl_id and a.seq_no = m.seq_no and a.co_id = m.co_id

left join (select distinct on (project_id, pbl_id, seq_no, co_id) project_id, pbl_id, seq_no, co_id, sum(tran_amt) as prin_paid
	from rf_jv_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and bal_side = 'C'
	and (jv_no, co_id) in (select jv_no, co_id from rf_jv_header where fiscal_yr = '2018')
	group by project_id, pbl_id, seq_no, co_id) mm on a.proj_id = mm.project_id and a.pbl_id = mm.pbl_id and a.seq_no = mm.seq_no and a.co_id = mm.co_id

left join (select distinct on (proj_id, pbl_id, seq_no, co_id) proj_id, pbl_id, seq_no, co_id, sum(trans_amt) as prin_paid
	from rf_crb_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and trans_amt < 0 and rb_fiscal_year = '2019'
	group by proj_id, pbl_id, seq_no, co_id) n on a.proj_id = n.proj_id and a.pbl_id = n.pbl_id and a.seq_no = n.seq_no and a.co_id = n.co_id

left join (select distinct on (project_id, pbl_id, seq_no, co_id) project_id, pbl_id, seq_no, co_id, sum(tran_amt) as prin_paid
	from rf_jv_detail where acct_id in ( '01-02-01-000' , '01-02-02-000', '02-01-01-000' ) and status_id = 'A' and bal_side = 'C'
	and (jv_no, co_id) in (select jv_no, co_id from rf_jv_header where fiscal_yr = '2019')
	group by project_id, pbl_id, seq_no, co_id) nn on a.proj_id = nn.project_id and a.pbl_id = nn.pbl_id and a.seq_no = nn.seq_no and a.co_id = nn.co_id

left join (select pmt_scheme_id, unnest(type_id) as type_id  from mf_payment_scheme) i on e.pmt_scheme_id = i.pmt_scheme_id

left join mf_buyer_type j on i.type_id = j.type_id

left join (select a.rb_id, b.issued_date, a.co_id, a.proj_id, a.pbl_id, a.seq_no
	from ( select distinct on (rb_id, co_id, pay_rec_id, pbl_id, seq_no) rb_id, co_id, proj_id, pay_rec_id, pbl_id, seq_no from rf_crb_detail
	where acct_id = '06-01-01-000' and trans_amt < 0 and status_id = 'A' ) a
	left join rf_crb_header b on a.rb_id = b.rb_id and a.co_id = b.co_id and a.pay_rec_id = b.pay_rec_id ) O on a.proj_id = O.proj_id and a.pbl_id = O.pbl_id and a.seq_no = O.seq_no and a.co_id = O.co_id


order by b.entity_name]]>
