	<queryString>
		<![CDATA[select trim(a.acct_id)as acct_id,
length(a.acct_id) ,
(case when a.acct_level=1 then upper(trim(a.acct_name)) else
(case when a.acct_level=2 then ('     '||upper(trim(a.acct_name))) else
(case when a.acct_level=3 then ('         '||upper(trim(a.acct_name))) else
(case when a.acct_level=4 then ('             '||upper(trim(a.acct_name))) else '' end ) end)  end) end)  as acct_name,
(case when a.dbt_cdt = 'D' then 'Debit' else 'Credit' end)as dbt_cdt,
(case when a.status_id = 'A' then 'Active' else 'Inactive' end)as status_id,
to_char(a.date_created,'MM-dd-yyyy') as date_created,
(upper(trim(c.last_name))||', '||substring(c.first_name,1,1)||'.') as created_by

from mf_boi_chart_of_accounts a
left join em_employee b on a.created_by = b.emp_code
left join rf_entity c on b.entity_id = c.entity_id

where a.status_id = 'A'

order by a.acct_id
]]>
	</queryString>
