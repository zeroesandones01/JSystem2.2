	<queryString>
		<![CDATA[select a.*,
(case when coalesce(a.technical_desc, '') = '' then '' else b.technical_desc end) as tech_desc,
client_full_name || (case when coalesce(coborrower_full_name, '') != '' then ' / ' || coborrower_full_name else '' end) as borcobor,
(
	case
		when (a.coborrower_full_name is not null or coalesce(a.coborrower_full_name, '') != '') and (coalesce(a.coborrower_full_name, '') = coalesce(a.client_spouse_full_name, ''))
			then coalesce(a.client_spouse_full_name, '') || (case when coalesce(a.client_spouse_full_name, '') != '' and coalesce(c_coborrower_spouse_full_name, '') != '' then ' / ' || coalesce(a.client_full_name, '') else '' end)
		when (a.coborrower_full_name is not null or coalesce(a.coborrower_full_name, '') != '') and (coalesce(a.coborrower_full_name, '') != coalesce(a.client_spouse_full_name, ''))
			then coalesce(a.client_spouse_full_name, '') || (case when coalesce(a.client_spouse_full_name, '') != '' and coalesce(c_coborrower_spouse_full_name, '') != '' then ' / ' else '' end) || coalesce(c_coborrower_spouse_full_name, '')
		else a.client_spouse_full_name
	end
) as final_spouse
from view_hdmf_cts_flh240a ($P{01_clientID}, $P{02_projID}, $P{03_pblID}, $P{04_seqno}) a
left join mf_unit_info b on a.proj_id = b.proj_id and a.pbl_id = b.pbl_id]]>
	</queryString>
