	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="11"/>
	<property name="ireport.y" value="0"/>
	<parameter name="prepared_by" class="java.lang.String" isForPrompting="false"/>
	<parameter name="filterproject" class="java.lang.Boolean" isForPrompting="false"/>
	<parameter name="proj_id" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[select trim(a.compl_no) as complaint_no, a.date_complained,
get_client_name(entity_id) as client_name, get_employee_name(a.comp_rcvd_by) as received_by,
trim(format('%s-%s-%s'::text, btrim(e.phase::text), btrim(e.block::text), btrim(e.lot::text))::character varying) as pbl , trim(get_project_name(a.projcode)) as proj_name,
trim(b.dept_alias) as addressed_to, c.ctgry_desc, d.complain_source as complaint_source
from rf_client_complaints a
left join mf_department b on b.dept_code = a.comp_addressed_to
left join mf_complaint_category c on c.ctgry_code = a.category_code
left join mf_complaint_sources d on d.src_id = a.src_id
left join mf_unit_info e on e.pbl_id = a.pbl_id
WHERE compl_no not in (select compl_no from rf_complain_accomplishment)
--filters the complaints based on the project id
AND (CASE WHEN $P{filterproject} THEN a.projcode = $P{proj_id} ELSE TRUE END)
order by compl_no, date_complained]]>
	</queryString>
