	<queryString>
		<![CDATA[SELECT concat_ws(' ', concat_ws(', ', A.Last_Name, A.First_Name), A.Middle_Name) as entity_name, B.entity_name as broker_name, C.prc_id,
C.date_prc_id::DATE, C.hlurb_regist_no,
CASE
	WHEN RIGHT(TRIM(DATE_PART('day', A.out_date)::CHAR(200)), 1) = '1'
		THEN concat_ws('', TRIM(DATE_PART('day', A.out_date)::CHAR(2)), 'st'::CHAR(2))
	WHEN RIGHT(TRIM(DATE_PART('day', A.out_date)::CHAR(200)), 1) = '2'
		THEN concat_ws('', TRIM(DATE_PART('day', A.out_date)::CHAR(2)), 'nd'::CHAR(2))
	WHEN RIGHT(TRIM(DATE_PART('day', A.out_date)::CHAR(200)), 1) = '3'
		THEN concat_ws('', DATE_PART('day', A.out_date)::CHAR(2), 'rd'::CHAR(2))
	ELSE concat_ws('', TRIM(DATE_PART('day', A.out_date)::CHAR(2)), 'th'::CHAR(2))
END as Day,
TO_CHAR(to_timestamp (DATE_PART('month', A.out_date)::CHAR::text, 'MM'), 'Month') as Month,
RIGHT(DATE_PART('YEAR', A.out_date)::CHAR(4), 2) as Year
FROM rf_gov_doc_delta A
LEFT JOIN rf_entity B ON A.overriding_id = B.entity_id
LEFT JOIN rf_entity_id_no C ON A.overriding_id = C.entity_id
WHERE status = 'Released to BDT' AND tobeprinted = '1'
ORDER BY A.last_name, A.first_name, A.middle_name]]>
	</queryString>
