	<queryString>
		<![CDATA[select

a.pv_no ,
b.amount,
upper(trim(g.entity_name)) as created_by,
upper(trim(h.entity_name)) as last_edited_by,
to_char(a.date_created,'MM-dd-yyyy') as date_created,
to_char(a.date_posted,'MM-dd-yyyy') as date_posted,
trim(e.return_message) as return_message

from rf_pv_header a
left join (
	select distinct on (pv_no, co_id) a.pv_no, a.co_id, sum(tran_amt) as amount from (
	select pv_no, co_id, tran_amt  from rf_pv_detail where bal_side = 'D' and status_id = 'A' and co_id = $P{co_id} ) a
	group by pv_no, co_id  ) b on trim(a.pv_no) = trim(b.pv_no) and a.co_id = b.co_id
left join em_employee c on a.created_by=c.emp_code
left join em_employee d on a.edited_by=d.emp_code
left join (select * from rf_doc_return_message where status_id = 'A' and doc_id = '12' ) e on a.pv_no = e.doc_no and a.co_id=e.co_id
left join rf_entity g on c.entity_id = g.entity_id
left join rf_entity h on c.entity_id = h.entity_id

where a.status_id = $P{status_id}
and a.co_id = $P{co_id}
and a.proc_id = $P{proc_id}

and (case when $P{date_name} = 'pv_date' then a.pv_date::date else a.date_posted::date end) >= $P{date_from}
and (case when $P{date_name} = 'pv_date' then a.pv_date::date else a.date_posted::date end) - interval '1 day' <= $P{date_to}

order by a.pv_no]]>
	</queryString>
