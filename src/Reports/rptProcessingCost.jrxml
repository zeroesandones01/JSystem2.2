	<queryString>
		<![CDATA[select a.pbl_id, b.phase, b.block, b.lot, trim(get_client_name(a.entity_id)) as buyer, a.pcost_amt, a.tran_date, trim(get_client_name(d.entity_id)) as requestedby, a.rplf_no, c.pcostdtl_desc as part_desc, a.pcostid_dl, a.batch_no, (select count(a.pbl_id)
from rf_processing_cost a
left join mf_unit_info b on b.pbl_id = a.pbl_id
LEFT join mf_processing_cost_dl c on c.pcostdtl_id = a.pcostid_dl
LEFT JOIN em_employee d on a.requested_by = d.emp_code
WHERE a.batch_no = $P{batch_no}
AND a.status_id = 'A'
and b.phase = '1') as ph1,

(select count(a.pbl_id)
from rf_processing_cost a
left join mf_unit_info b on b.pbl_id = a.pbl_id
LEFT join mf_processing_cost_dl c on c.pcostdtl_id = a.pcostid_dl
LEFT JOIN em_employee d on a.requested_by = d.emp_code
WHERE a.batch_no = $P{batch_no}
AND a.status_id = 'A'
and b.phase = '2') as ph2,

(select count(a.pbl_id)
from rf_processing_cost a
left join mf_unit_info b on b.pbl_id = a.pbl_id
LEFT join mf_processing_cost_dl c on c.pcostdtl_id = a.pcostid_dl
LEFT JOIN em_employee d on a.requested_by = d.emp_code
WHERE a.batch_no = $P{batch_no}
AND a.status_id = 'A'
and b.phase = '4-A') as ph4a,

(select count(a.pbl_id)
from rf_processing_cost a
left join mf_unit_info b on b.pbl_id = a.pbl_id
LEFT join mf_processing_cost_dl c on c.pcostdtl_id = a.pcostid_dl
LEFT JOIN em_employee d on a.requested_by = d.emp_code
WHERE a.batch_no = $P{batch_no}
AND a.status_id = 'A'
and b.phase = '4') as ph4,

(select sum(a.pcost_amt)
from rf_processing_cost a
left join mf_unit_info b on b.pbl_id = a.pbl_id
LEFT join mf_processing_cost_dl c on c.pcostdtl_id = a.pcostid_dl
LEFT JOIN em_employee d on a.requested_by = d.emp_code
WHERE a.batch_no = $P{batch_no}
AND a.status_id = 'A'
and b.phase = '1') as ph1amt,


(select sum(a.pcost_amt)
from rf_processing_cost a
left join mf_unit_info b on b.pbl_id = a.pbl_id
LEFT join mf_processing_cost_dl c on c.pcostdtl_id = a.pcostid_dl
LEFT JOIN em_employee d on a.requested_by = d.emp_code
WHERE a.batch_no = $P{batch_no}
AND a.status_id = 'A'
and b.phase = '2') as ph2amt,

(select sum(a.pcost_amt)
from rf_processing_cost a
left join mf_unit_info b on b.pbl_id = a.pbl_id
LEFT join mf_processing_cost_dl c on c.pcostdtl_id = a.pcostid_dl
LEFT JOIN em_employee d on a.requested_by = d.emp_code
WHERE a.batch_no = $P{batch_no}
AND a.status_id = 'A'
and b.phase = '4-A') as ph4amt,

(select sum(a.pcost_amt)
from rf_processing_cost a
left join mf_unit_info b on b.pbl_id = a.pbl_id
LEFT join mf_processing_cost_dl c on c.pcostdtl_id = a.pcostid_dl
LEFT JOIN em_employee d on a.requested_by = d.emp_code
WHERE a.batch_no = $P{batch_no}
AND a.status_id = 'A'
and b.phase = '4') as ph4mt,

a.remarks
from rf_processing_cost a
left join mf_unit_info b on b.pbl_id = a.pbl_id
LEFT join mf_processing_cost_dl c on c.pcostdtl_id = a.pcostid_dl
LEFT JOIN em_employee d on a.requested_by = d.emp_code
WHERE $X{IN,pcostid_dl,pcost_id}
AND (CASE WHEN ($P{projcode} = 'All' OR NULLIF(UPPER($P{projcode}), 'NULL') IS NULL) THEN TRUE ELSE a.projcode ~* $P{projcode} END)
AND (CASE WHEN ($P{batch_no} = 'All' OR NULLIF(UPPER($P{batch_no}), 'NULL') IS NULL) THEN TRUE ELSE a.batch_no ~* $P{batch_no} END)
AND (CASE WHEN ($P{batch_no1} = 'All' OR NULLIF(UPPER($P{batch_no1}), 'NULL') IS NULL) THEN TRUE ELSE a.batch_no ~* $P{batch_no1} END)
AND (CASE WHEN ($P{buyer} = 'All' OR NULLIF(UPPER($P{buyer}), 'NULL') IS NULL) THEN TRUE ELSE a.entity_id ~* $P{buyer} END)
AND a.tran_date between $P{date_from} and $P{date_to}
AND a.status_id = 'A']]>
