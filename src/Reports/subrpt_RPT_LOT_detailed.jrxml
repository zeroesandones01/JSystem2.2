<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="subrpt_RPT_LOT_detailed" pageWidth="154" pageHeight="135" columnWidth="154" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="2.7272727272727275"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="entity_id" class="java.lang.String"/>
	<parameter name="proj_id" class="java.lang.String"/>
	<parameter name="pbl_id" class="java.lang.String"/>
	<parameter name="seq_no" class="java.lang.String"/>
	<queryString>
		<![CDATA[Select coalesce(a.rpt_year, EXTRACT(YEAR FROM a.tran_date::DATE ))::INT as rpt_year, a.pcost_amt as rpt_amount
	from rf_processing_cost a
	where TRIM(entity_id)  = $P{entity_id}
	and TRIM(projcode) = $P{proj_id}
	and TRIM(pbl_id) = $P{pbl_id}
	and seq_no::VARCHAR  = $P{seq_no}
	and TRIM(a.pcostid_dl) = '216'
	and trim(a.status_id) = 'A'
	and COALESCE(a.rpt_year, EXTRACT(YEAR FROM a.tran_date::DATE)) >
	(Select EXTRACT(YEAR FROM actual_date::DATE)
	 from rf_buyer_status
	 where TRIM(entity_id)  = $P{entity_id}
	 and TRIM(proj_id) = $P{proj_id}
	 and TRIM(pbl_id) = $P{pbl_id}
	 and seq_no::VARCHAR  = $P{seq_no}
	 and TRIM(status_id) = 'A'
	and TRIM(byrstatus_id) IN ('32', '1F'))
	order by rpt_year]]>
	</queryString>
	<field name="rpt_year" class="java.lang.Integer"/>
	<field name="rpt_amount" class="java.math.BigDecimal"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<detail>
		<band height="12" splitType="Stretch">
			<textField isBlankWhenNull="true">
				<reportElement x="0" y="0" width="72" height="12"/>
				<box leftPadding="0"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="SansSerif" size="8" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{rpt_year}]]></textFieldExpression>
			</textField>
			<textField pattern="#,##0.00" isBlankWhenNull="false">
				<reportElement x="72" y="0" width="82" height="12"/>
				<box leftPadding="0"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="SansSerif" size="8" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.math.BigDecimal"><![CDATA[$F{rpt_amount}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
