	<queryString>
		<![CDATA[select * from (

select
row_number() over(PARTITION BY a.co_id ORDER BY gg.entity_name) as row_num,
upper(trim(gg.entity_name)) as client,
h.proj_alias,
to_char(a.deposit_date, 'MM-dd-yyyy') as dep_date,
trim(i.description) as pbl,
trim(j.dept_alias) as dept,
m.bank_alias,
trim(a.bank_account) as bank_acct,
trim(n.particulars) as pmt_part,
a.amount,
a.record_id,
a.co_id

from tf_unidentified_dep a
left join em_employee e on a.tagged_by = e.emp_code
left join rf_entity ee on e.entity_id = ee.entity_id
left join em_employee f on a.identified_by = f.emp_code
left join rf_entity ff on f.entity_id = ff.entity_id
left join rf_sold_unit g on a.entity_id = g.entity_id and a.proj_id = g.projcode and a.pbl_id = g.pbl_id and a.seq_no = g.seq_no
left join rf_entity gg on g.entity_id = gg.entity_id
left join mf_project h on g.projcode = h.proj_id
left join mf_unit_info i on g.projcode = i.proj_id and g.pbl_id = i.pbl_id
left join mf_department j on g.mktgarm = j.dept_code
left join mf_bank_account k on a.bank_acct_id = k.bank_acct_id
left join mf_bank_branch l on k.bank_branch_id = l.bank_branch_id
left join mf_bank m on l.bank_id = m.bank_id
left join mf_pay_particular n on a.payment_part = n.pay_part_id

where a.status_id = 'F'
and a.co_id = $P{co_id}
and a.identified_date >= $P{date_from}
and a.identified_date - interval '1 day' < $P{date_to}

order by gg.entity_name

) a

order by a.row_num]]>
	</queryString>
