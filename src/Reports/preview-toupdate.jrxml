	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="3"/>
	<property name="ireport.y" value="28"/>
	<parameter name="company_name" class="java.lang.String"/>
	<parameter name="logo" class="java.io.InputStream"/>
	<parameter name="co_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select rpad(b.entity_name,20, ' ') as client_name,
(right(a.entity_id,10)
 || lpad(to_char(a.pbl_id::int,'FM00000'),5)
 || lpad(to_char(c.proj_id::int,'FM00000'),5)) as reference,
'BDP' as particular,
a.amt_toupdate
from rf_card_pmt_toupdate a
left join rf_entity b on a.entity_id = b.entity_id
left join mf_project c on a.proj_id = c.proj_id
left join mf_unit_info d on a.pbl_id = d.pbl_id
where (case when $P{co_id} != '' then c.co_id = $P{co_id} else true end)
order by b.entity_name, c.proj_name]]>
	</queryString>
	<field name="client_name" class="java.lang.String"/>
	<field name="reference" class="java.lang.String"/>
	<field name="particular" class="java.lang.String"/>
	<field name="amt_toupdate" class="java.math.BigDecimal"/>
