	<queryString>
		<![CDATA[select

(case when row_num = 1 then a.rplf_no else '' end) as rplf_no,
(case when row_num = 1 then a.rplf_date else null end) as rplf_date,
(case when row_num = 1 then a.date_needed  else null end) as date_needed,
(case when row_num = 1 then a.payee else '' end) as payee,
a.part_desc,
a.availer,
a.account,
a.amount,
a.status_desc,
a.status_id,
a.line_no

from (

select

row_number() over(PARTITION BY a.rplf_no, b.rplf_date, b.date_needed, c.entity_name ORDER BY a.rplf_no, b.rplf_date, b.date_needed, c.entity_name) as row_num,
a.rplf_no,
a.line_no,
b.rplf_date,
b.date_needed,
upper(trim(c.entity_name)) as payee,
a.part_desc,
upper(trim(d.entity_name)) as availer,
(a.acct_id||'('||trim(e.acct_name)||')') as account,
a.amount,
( case when b.status_id = 'I' then 'INACTIVE' else
		( case when b.status_id = 'D' then 'DELETED' else
		( case when b.status_id = 'A' and g.pv_no is null then 'ACTIVE' else
		( case when b.status_id = 'A' and g.pv_no is not null then 'PROCESSED' else '' end) end) end) end) as status_desc,
( case when b.status_id = 'I' then 'I' else
		( case when b.status_id = 'D' then 'D' else
		( case when b.status_id = 'A' and g.pv_no is null then 'A' else
		( case when b.status_id = 'A' and g.pv_no is not null then 'P' else '' end) end) end) end) as status_id


from (select * from rf_request_detail where status_id != 'I' order by rplf_no, line_no) a
left join rf_request_header b on a.rplf_no = b.rplf_no and a.co_id = b.co_id
left join rf_entity c on b.entity_id1=c.entity_id
left join rf_entity d on a.entity_id=d.entity_id
left join mf_boi_chart_of_accounts e on a.acct_id = e.acct_id
left join rf_pv_header g on a.rplf_no = g.pv_no and a.co_id = g.co_id

where a.rplf_no != ''
and b.rplf_date::date >= $P{date_from}
and b.rplf_date::date <= $P{date_to}
and b.co_id = $P{co_id}
and ( case when $P{req_type_id} = '' or $P{req_type_id} is null then a.rplf_no is not null else b.rplf_type_id = $P{req_type_id}  end )
and ( case when $P{payee_id} = '' or $P{payee_id} is null then a.rplf_no is not null else b.entity_id1 = $P{payee_id}  end )
and ( case when $P{availer_id} = '' or $P{availer_id} is null then a.rplf_no is not null else a.entity_id = $P{availer_id}  end )
and ( case when $P{acct_id} = '' or $P{acct_id} is null then a.rplf_no is not null else a.acct_id = $P{acct_id}  end )
and ( case when $P{prepared_id} = '' or $P{prepared_id} is null then a.rplf_no is not null else b.created_by = $P{prepared_id}  end )
and ( case when $P{branch_id} = '' or $P{branch_id} is null then a.rplf_no is not null else b.branch_id = $P{branch_id}  end )
and ( case when $P{drf_fr} = 0 or $P{drf_fr} is null then a.rplf_no is not null else b.rplf_no::int >= $P{drf_fr}   end )
and ( case when $P{drf_to} = 0 or $P{drf_to} is null then a.rplf_no is not null else b.rplf_no::int <= $P{drf_to}  end )

) a

where (case when $P{status_id} = '' then a.status_id is not null else a.status_id = $P{status_id} end )]]>
	</queryString>
