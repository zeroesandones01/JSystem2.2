	<parameter name="pymnt_type" class="java.lang.String"/>
	<parameter name="co_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select

ax.acct_id,
--(case when a.acct_id = '01-01-01-002' then 'CASH ON HAND DISBURSEMENT' else a.bank_name end) bank_name,
(case when ax.acct_id = '01-01-01-002' then 'CASH ON HAND DISBURSEMENT'
	else (select
	c.bank_name
	from mf_bank_account a
	left join mf_bank_branch b on a.bank_branch_id = b.bank_branch_id
	left join mf_bank c on b.bank_id = c.bank_id
	where a.acct_id = ax.acct_id and a.status_id != 'I'
          limit 1 ) end)
as bank_name,
--(case when a.acct_id = '01-01-01-002' then '-' else a.bank_acct_no end) as bank_acct_no,
(case when ax.acct_id = '01-01-01-002' then '-'
	else (select
	a.bank_acct_no
	from mf_bank_account a
	left join mf_bank_branch b on a.bank_branch_id = b.bank_branch_id
	left join mf_bank c on b.bank_id = c.bank_id
	where a.acct_id = ax.acct_id
	and a.status_id != 'I'
          limit 1) end)
as bank_acct_no,
sum(ax.amount) as amount

from (
select

(select x.acct_id from rf_cv_detail x where x.cv_no = a.cv_no and x.co_id = a.co_id and bal_side = 'C' and x.acct_id not in ( '00301004', '', '03-01-01-001') limit 1) as acct_id,
to_char(b.date_due,'MM-dd-yyyy') as check_date,
b.check_no,
a.cv_no,
to_char(a.cv_date,'MM-dd-yyyy') as dv_date,
to_char(a.date_paid,'MM-dd-yyyy') as date_paid,
trim(c.entity_name) as payee,
--b.chk_amt
(case when b.chk_amt is null then (select tran_amt from rf_cv_detail where cv_no = a.cv_no and bal_side = 'C' and status_id = 'A' and co_id = $P{co_id}) else b.chk_amt end) as amount
from rf_cv_header a
left join (select * from rf_check where status_id = 'A') b on a.cv_no = b.cv_no and a.co_id = b.co_id
left join rf_entity c on coalesce(b.entity_id, a.entity_id1) = c.entity_id

where
(case when $P{pymnt_type} = '' then true else a.pay_type_id = $P{pymnt_type} end)
and (case when $P{payee_id} = '' then a.entity_id1 is not null else coalesce(b.entity_id, a.entity_id1) = $P{payee_id} end)
and (case when $P{excl_payee_id} = '' then true else coalesce(b.entity_id, a.entity_id1) != $P{excl_payee_id} end)
and a.status_id = 'P'
and a.date_paid >= $P{date_from}::date
and a.date_paid <= $P{date_to}::date
and a.proc_id in (5,6,7,14,15,16,17)
and a.co_id = $P{co_id}
--and not exists (select * from rf_cv_detail where cv_no = a.cv_no and acct_id in ( '00301004', '', '03-01-01-001') and bal_side = 'C' and status_id = 'A')
and exists (select * from rf_cv_detail where cv_no = a.cv_no and (case when $P{acct_id} = '' then true else acct_id = $P{acct_id} end) and bal_side = 'C' and status_id = 'A')

) ax

group by ax.acct_id
--a.bank_name,
--a.bank_acct_no]]>
	</queryString>
