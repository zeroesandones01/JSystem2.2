	<queryString>
		<![CDATA[select * from (

select

'PV' as book,
a.pv_no,
b.date_posted,
to_char(b.date_posted, 'yyyy') as pv_year,
to_char(b.date_posted, 'MM')::int as pv_month,
trim(to_char(b.date_posted, 'Month')) as pv_month_word,
pv_amt,
trim(b.remarks) as remarks,
trim(acct_name) as acct_name

from (select pv_no, co_id, (case when bal_side = 'D' then 1 else -1 end)*tran_amt as pv_amt, acct_id from rf_pv_detail
	where acct_id like '08-01%'
	and acct_id not like '08-01-32%'
	and status_id = 'A'
	and case when $P{proj_id} = '' then pv_no is not null else project_id = $P{proj_id} end
	and case when $P{phase_no} = 'ALL' then pv_no is not null else sub_projectid = $P{phase} end
	) a
join (select * from rf_pv_header where date_posted is not null) b on a.co_id = b.co_id and a.pv_no = b.pv_no
left join mf_boi_chart_of_accounts c on a.acct_id = c.acct_id

UNION ALL

select

'CV' as book,
a.cv_no,
b.date_posted,
to_char(b.date_posted, 'yyyy') as cv_year,
to_char(b.date_posted, 'MM')::int as cv_month,
trim(to_char(b.date_posted, 'Month')) as cv_month_word,
cv_amt,
trim(b.remarks) as remarks,
trim(acct_name) as acct_name

from (select cv_no, co_id, (case when bal_side = 'D' then 1 else -1 end)*tran_amt as cv_amt, acct_id  from rf_cv_detail
	where acct_id like '08-01%'
	and acct_id not like '08-01-32%'
	and status_id = 'A'
	) a
join (select * from rf_cv_header where date_posted is not null) b on a.co_id = b.co_id and a.cv_no = b.cv_no
left join mf_boi_chart_of_accounts c on a.acct_id = c.acct_id

UNION ALL

select

'JV' as book,
a.jv_no,
b.date_posted,
to_char(b.date_posted, 'yyyy') as jv_year,
to_char(b.date_posted, 'MM')::int as jv_month,
trim(to_char(b.date_posted, 'Month')) as jv_month_word,
jv_amt,
trim(b.remarks) as remarks,
trim(acct_name) as acct_name

from (select jv_no, co_id, (case when bal_side = 'D' then 1 else -1 end)*tran_amt as jv_amt, acct_id from rf_jv_detail
	where acct_id like '08-01%'
	and acct_id not like '08-01-32%'
	and status_id = 'A'
	and case when $P{proj_id} = '' then jv_no is not null else project_id = $P{proj_id} end
	and case when $P{phase_no} = 'ALL' then jv_no is not null else sub_projectid = $P{phase} end
	) a
join (select * from rf_jv_header where date_posted is not null) b on a.co_id = b.co_id and a.jv_no = b.jv_no
left join mf_boi_chart_of_accounts c on a.acct_id = c.acct_id

) A

where A.pv_year::text = $P{year}
and A.pv_month::int >= $P{period_id_from}::int
and A.pv_month::int <= $P{period_id_to}::int

order by A.pv_month, A.date_posted]]>
	</queryString>
