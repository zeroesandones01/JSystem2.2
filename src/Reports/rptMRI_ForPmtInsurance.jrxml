	<queryString>
		<![CDATA[SELECT e.company_name as company_name,b.rplf_no as rplf_no, b.part_id || ' STAGE' as pmt_stage,
get_project_alias(a.proj_id) as proj_alias, c.description as unit_desc,
get_client_name(a.entity_id) as client_name, b.date_enrolled as date_enrolled, b.date_approved as date_approved,
b.policy_no as policy_no, trim(b.reference_no) as reference_no, coalesce(b.invoice_no, '') as invoice_no, b.amt_insured as amt_insured, b.premium as premium
FROM rf_mri_header a
LEFT JOIN rf_mri_details b on b.applicant_no = a.applicant_no
LEFT JOIN mf_unit_info c on c.proj_id = a.proj_id AND c.pbl_id = a.pbl_id
LEFT JOIN mf_project d on d.proj_id = a.proj_id
LEFT JOIN mf_company e on e.co_id = d.co_id
WHERE b.invoice_no = $P{invoice_no}
and b.rplf_no = $P{rplf_no}
AND b.date_approved IS NOT NULL
AND b.date_terminated IS NULL
AND b.termination_no IS NULL
AND a.proj_id = $P{proj_id}
ORDER BY b.invoice_no;]]>
	</queryString>
