	<queryString>
		<![CDATA[select
	trim(f.entity_name)as entity_name,
	g.proj_alias,
	trim(a.pbl_id)as pbl_id,
	trim(e.description) as desc,
	a.seq_no,
	k.particulars,
	a.amount as amount,
	coalesce(b.bank_alias,'') as bank_name,
	a.check_no,
	to_char(a.check_date,'MM/dd/yy')as check_date ,
	d.checkstat_desc,
	h.reason_desc,
	to_char(a.trans_date,'MM/dd/yy')as actual_date,
	to_char(i.sor_date,'MM/dd/yy')as sor_date,
	to_char(j.bb_date,'MM/dd/yy')as bb_date,
	trim(a.brstn) as brstn

from (select * from rf_payments x where x.check_date::date <= get_next_bank_day(now()::date) and x.check_stat_id not in ('02', '01') and status_id != 'I') a

left join mf_bank b
on a.bank_id = b.bank_id

left join mf_bank_branch c
on a.bank_branch_id = c.bank_branch_id

left join mf_check_status d
on a.check_stat_id = d.checkstat_id

left join mf_unit_info e
on a.pbl_id = e.pbl_id

left join rf_entity f
on a.entity_id = f.entity_id

left join mf_project g
on a.proj_id = g.proj_id

left join mf_check_bounce_reason h
on a.bounce_reason_id = h.reason_id

left join (select distinct on (pbl_id, seq_no) pbl_id, seq_no, tran_date as sor_date from rf_buyer_status
 where trim(byrstatus_id) in ('25','52','67','2A')) i on a.pbl_id=i.pbl_id and a.seq_no=i.seq_no

left join (select distinct on (pbl_id, seq_no) pbl_id, seq_no, tran_date as bb_date from rf_buyer_status
 where trim(byrstatus_id) in ('26')) j on a.pbl_id=j.pbl_id and a.seq_no=j.seq_no

left join mf_pay_particular k on a.pay_part_id = k.pay_part_id

where a.pymnt_type = 'B'

and ( case when $P{proj_id} = '' or $P{proj_id} is null then true else a.proj_id = $P{proj_id}  end )

and ( case when $P{phase} = '' or $P{phase} is null then true else e.phase = $P{phase}  end )

and a.check_date >= $P{date_from}

and a.check_date <= $P{date_to}

and ( case when $P{filter_by} = 'Not Yet Sold to Bank' then i.sor_date is null else i.sor_date is not null end )

and ( case when $P{branch_id} = '' or $P{branch_id} is null then true else a.branch_id = $P{branch_id}  end )]]>
	</queryString>
