	<parameter name="period_id_to" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT * FROM (

select

a.pbl_id,
a.seq_no,
trim(description) as unit_desc,
trim(a.entity_id) as clientid,
trim(b.entity_name) as client_name,
trim(e.tin_no) as tin_no,
a.rb_fiscal_year,
a.rb_month::int,
trim(to_char(d.issued_date,'Month')) as month,
a.cres_amt as cres_amt

from (select proj_id, phase, pbl_id, entity_id, seq_no, co_id, trans_amt as cres_amt, rb_id,  pay_rec_id, doc_id, rb_fiscal_year, rb_month
	from rf_crb_detail where acct_id like '07%' and status_id = 'A') a
left join rf_entity b on a.entity_id = b.entity_id
left join mf_unit_info c on a.pbl_id = c.pbl_id and a.proj_id = c.proj_id
left join rf_crb_header d on a.rb_id = d.rb_id and a.doc_id = d.doc_id and a.pay_rec_id = d.pay_rec_id
left join rf_entity_id_no e on a.entity_id = e.entity_id

where a.rb_fiscal_year::text = $P{year}
and case when $P{proj_id} = '' then a.rb_id is not null else a.proj_id = $P{proj_id} end
and case when $P{phase_no} = 'ALL' then a.rb_id is not null else a.phase = $P{phase_no} end
and rb_month::int >= $P{period_id_from}::int
and rb_month::int <= $P{period_id_to}::int
and d.status_id in ('A','P')


UNION ALL

--FROM JV

select


a.pbl_id,
a.seq_no,
trim(description) as unit_desc,
trim(a.entity_id) as entity_id,
trim(d.entity_name) as client_name,
trim(e.tin_no) as tin_no,
b.fiscal_yr as jv_year,
b.period_id::int as jv_month,
to_char(b.jv_date, 'Month') as jv_month_word,
a.jv_amt

from (select jv_no, co_id, (case when bal_side = 'D' then 1 else -1 end)*tran_amt as jv_amt, entity_id, project_id, sub_projectid, pbl_id, seq_no
	from rf_jv_detail where acct_id like '07%' and status_id = 'A') a
join (select * from rf_jv_header where status_id in ('A','P') ) b on a.co_id = b.co_id and a.jv_no = b.jv_no
left join mf_unit_info c on a.pbl_id = c.pbl_id and a.project_id = c.proj_id
left join rf_entity d on a.entity_id = d.entity_id
left join rf_entity_id_no e on a.entity_id = e.entity_id

where b.fiscal_yr::text = $P{year}
and case when $P{proj_id} = '' then a.jv_no is not null else a.project_id = $P{proj_id} end
and case when $P{phase_no} = 'ALL' then a.jv_no is not null else a.sub_projectid = $P{phase} end
and period_id::int >= $P{period_id_from}::int
and period_id::int <= $P{period_id_to}::int

) A

order by a.rb_month::int, a.pbl_id::int, a.seq_no]]>
