	<queryString>
		<![CDATA[select false,
object_id,
object_name,
e.category_name,
date_acquired::date,
current_cust,
c.entity_name,
a.status_id, a.rec_id, a.remarks, a.dept_code
from tbl_nonfixedasset a
left join em_employee b on a.current_cust = b.emp_code
left join rf_entity c on b.entity_id = c.entity_id
left join tbl_item d on a.item_id::int = d.item_id
left join tbl_category e on d.category_id = e.category_id
where a.status_id = 'Active' order by object_id asc]]>
