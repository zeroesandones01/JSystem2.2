	<parameter name="assetToPrint" class="java.lang.String"/>
	<parameter name="employee_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
a.asset_code,
a.asset_no::varchar,
a.asset_name,
to_char(a.asset_no, 'FM00000000') as anum,
c.entity_name as nme,
current_date,
lower(format('%s%s%s',left(c.first_name,1),left(c.middle_name,1),c.last_name)) as cuser
from tbl_asset  a
left join em_employee b on a.current_cust::varchar=b.emp_code
left join rf_entity as c on b.entity_id=c.entity_id
where a.asset_no::int=$P{assetToPrint}::int and b.emp_code= $P{employee_id}
order by a.asset_no]]>
	</queryString>
