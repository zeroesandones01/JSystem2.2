	</parameter>
	<parameter name="pbl_id" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["241"]]></defaultValueExpression>
	</parameter>
	<parameter name="seq_no" class="java.lang.Integer" isForPrompting="false">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT --sp_center_format(30, FORMAT('%s %s', TRIM(c.first_name), TRIM(c.last_name))) as client_name,
sp_center_format(0, TRIM(f.company_name)) as company_name,
f.company_address,
sp_center_format(0, TRIM(d.proj_name)) as proj_name,
sp_center_format(0, TRIM('JOHNNY LIWANAG CORPUZ')) as sir_johnny,
sp_center_format(0, FORMAT('%s %s. %s', TRIM(c.first_name), substr(TRIM(c.middle_name), 1, 1), TRIM(c.last_name))) as client_name,
sp_center_format(0, FORMAT('%s %s', TRIM(g.connection_first_name), TRIM(g.connection_last_name))) as client_spouse,
sp_center_format(0, COALESCE(get_client_address(a.entity_id), 'N/A')) as client_address,
sp_center_format(0, COALESCE(NULLIF(TRIM(FORMAT('%s %s', TRIM(k.connection_first_name), TRIM(k.connection_last_name))), ''), 'N/A')) as coapplicant,
sp_center_format(0, COALESCE(NULLIF(TRIM(FORMAT('%s %s', TRIM(m.connection_first_name), TRIM(m.connection_last_name))), ''), 'N/A')) as coapplicant_spouse,
sp_center_format(0, COALESCE(get_client_address(k.entity_id), 'N/A')) as coapplicant_address,
FORMAT('%s AND XX / 100', to_words(i.amount)) as tr_words,
i.amount as temporary_reservation,
FORMAT('%s AND XX / 100', to_words(j.amount)) as or_words,
j.amount as official_reservation,
b.phase, b.block, b.lot, b.lotarea, e.model_desc, a.sellingprice, o.client_id_no, o.client_id_presented, o.client_tin_no, o.signatory_name, o.signatory_id_presented, o.signatory_id_no, o.signatory_tin_no, o.signatory_position,
get_acts_no(a.entity_id, a.projcode, a.pbl_id, a.seq_no) as acts_no

FROM rf_sold_unit a
LEFT JOIN mf_unit_info b ON b.proj_id = a.projcode and b.pbl_id = a.pbl_id
LEFT JOIN rf_entity c ON c.entity_id = a.entity_id
LEFT JOIN mf_project d ON d.proj_id = a.projcode
LEFT JOIN mf_product_model e ON e.model_id = a.model_id
LEFT JOIN mf_company f ON f.co_id = d.co_id

--Spouse
LEFT JOIN rf_entity_connect g ON g.entity_id = a.entity_id and g.connect_type = 'SP' AND g.status_id = 'A'

LEFT JOIN mf_pay_scheme_detail i ON i.pmt_scheme_id  = a.pmt_scheme_id AND i.part_id = '012' AND i.part_sequence = 1
LEFT JOIN mf_pay_scheme_detail j ON j.pmt_scheme_id  = a.pmt_scheme_id AND j.part_id = '012' AND j.part_sequence = 2

--Co-Applicant
LEFT JOIN rf_entity_connect k ON k.entity_id = a.entity_id AND k.connect_type = 'CO' AND k.status_id = 'A'

--Spouse
LEFT JOIN rf_entity_connect m ON m.entity_id = k.entity_id AND m.connect_type = 'SP' AND m.status_id = 'A'

LEFT JOIN (SELECT * FROM view_acknowledgement_part($P{entity_id}, $P{proj_id}, $P{pbl_id}, $P{seq_no})) o ON o.entity_id = a.entity_id

WHERE a.entity_id = $P{entity_id}
AND a.projcode = $P{proj_id}
AND a.pbl_id = $P{pbl_id}
AND a.seq_no = $P{seq_no};]]>
	</queryString>
