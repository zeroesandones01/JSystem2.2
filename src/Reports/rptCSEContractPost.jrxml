		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="subrptCSEContractUnits" class="java.io.InputStream" isForPrompting="false"/>
	<parameter name="_server" class="java.lang.String" isForPrompting="false"/>
	<parameter name="_dbase" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT
a.award_date,
a.contract_no,
trim(b.entity_name) as entity_name,
initcap(trim(b.address)) as address,
initcap(trim(b.contactperson)) as contactperson,
trim(b.position) as position,
b.tin,
initcap(trim(c.proj_name)) as proj_name,
trim(c.barangay) as proj_barangay,
trim(c.municipality) as proj_municipality,
trim(c.province) as proj_province,
d._count,
to_words(d._count::VARCHAR) AS  _word,
f.contract_price as orig_contract_price,
initcap(TRIM(REPLACE(cash_words(f.contract_price::VARCHAR::MONEY), '  dollars', ''))) AS orig_contract_price_word,
a.dp_percentage,
to_words(a.dp_percentage::VARCHAR) as dp_percentage_word,
(a.orig_contract_price * a.dp_percentage) /100 as downpayment,
initcap(TRIM(REPLACE(cash_words(((a.orig_contract_price * a.dp_percentage)/ 100)::VARCHAR::MONEY), '  dollars', ''))) as downpayment_word,
a.start_date,
a.finish_date,
date_part('day', timestamp_mi(timestamp_pl_interval(a.finish_date, INTERVAL '1 day'), a.start_date))::INT AS working_days,
initcap(to_words(date_part('day', timestamp_mi(timestamp_pl_interval(a.finish_date, INTERVAL '1 day'), a.start_date))::VARCHAR)) AS  working_days_word,
initcap(trim(e.co_name)) as company,
trim(e.bldg_street) || ' ' || trim(e.district_municipality) || ' ' || trim(city_province) as company_address,
(select (select initcap(sub_proj_name) from mf_sub_project where proj_id = a.proj_id and sub_proj_alias = a.phase)
	from (select regexp_split_to_table(contract_no, '-') as phase, proj_id
		from co_ntp_header
		where ntp_no = $P{ntp_no} and _server = $P{_server} and _dbase = $P{_dbase}) a
	where a.phase ~* 'PH') as phase,
(select sum(work_qty) from co_ntp_detail where ntp_no = $P{ntp_no} and _server = $P{_server} and _dbase = $P{_dbase}) as units,
(select contract[array_length(contract)]
	from (select *, regexp_split_to_array(trim(contract_no), '-') as contract from co_ntp_header) a
	where ntp_no = $P{ntp_no} and _server = $P{_server} and _dbase = $P{_dbase}) as contract_type

FROM co_ntp_header a
LEFT JOIN mf_entity b ON b.entity_id = a.entity_id AND b._server = a._server and b._dbase = a._dbase
LEFT JOIN mf_project c ON c.proj_id = a.proj_id AND c._server = a._server and c._dbase = a._dbase
LEFT JOIN (SELECT count(ntp_no) AS _count, ntp_no
	FROM co_ntp_detail
	WHERE ntp_no = $P{ntp_no} AND _server = $P{_server} AND _dbase = $P{_dbase}
	GROUP BY ntp_no) d ON d.ntp_no = a.ntp_no
LEFT JOIN mf_company e ON e.co_id = a.co_id AND e._server = a._server and e._dbase = a._dbase
LEFT JOIN (SELECT ntp_no, sum(work_qty * other_cost) as contract_price
	FROM co_ntp_detail
	WHERE ntp_no = $P{ntp_no} and _server = $P{_server} and _dbase = $P{_dbase}
	GROUP BY ntp_no) f ON f.ntp_no = a.ntp_no

WHERE a.ntp_no = $P{ntp_no}
AND a._server = $P{_server}
AND a._dbase = $P{_dbase};]]>
	</queryString>
