	<queryString>
		<![CDATA[SELECT

rplf_date,
'' as reference_no,
trim(c.acct_name) as acct_desc,
trim(part_desc) as particular,
(case when a.with_budget = true then 'Yes' else 'No' end) as within_budget,
coalesce(d.division_alias,'')||
case when d.division_alias is null or e.dept_alias is null then ' ' else '-' end ||
coalesce(e.dept_alias,'')||' / '||coalesce(f.proj_alias,'')||
 case when nullif(g.phase,'') is null then '' else  '-PH'||coalesce(g.phase,'') end as cost_center,
a.amount,
b.date_needed,
upper(trim(h.entity_name)) as payee,
upper(trim(i.rplf_type_desc)) as rplf_type,
'/'||trim(k.entity_name) as prepared_by


FROM (select * from rf_request_detail where status_id != 'I') a
LEFT JOIN rf_request_header b ON a.co_id = b.co_id AND a.rplf_no = b.rplf_no
LEFT JOIN mf_boi_chart_of_accounts c on a.acct_id = c.acct_id
LEFT JOIN mf_division d on a.div_id = d.division_code
LEFT JOIN mf_department e on a.dept_id = e.dept_code
LEFT JOIN mf_project f on a.project_id = f.proj_id
LEFT JOIN mf_sub_project g on a.project_id = g.proj_id and a.sub_projectid = g.sub_proj_id
LEFT JOIN rf_entity h on b.entity_id1 = h.entity_id
LEFT JOIN mf_rplf_type i on b.rplf_type_id = i.rplf_type_id
LEFT JOIN em_employee j on (case when a.edited_by is null or a.edited_by = '' then a.created_by else a.edited_by end) = j.emp_code
LEFT JOIN rf_entity k on j.entity_id = k.entity_id


WHERE a.rplf_no = $P{rplf_no}
and a.co_id = $P{co_id}



order by a.ref_doc_no, a.line_no;]]>
	</queryString>
