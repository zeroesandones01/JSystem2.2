	<queryString>
		<![CDATA[select *, now()::date as date,
(select x.hdmf_no from rf_entity_id_no x where x.entity_id = a.entity_id) as hdmf_mid_no
from view_hdmf_transmittal($P{03_CoID}, $P{04_ProID}, $P{05_Phase}) a
where a.main_client in
(
	select x.client_name
	from tmp_hdmf x
	where x.emp_code = $P{09_UserID}
)
order by a.last_name, a.first_name, a.middle_name]]>
	</queryString>
