	<queryString>
		<![CDATA[select
seq_no,
coalesce(nullif(phase, '-'), (select trim(trim(trim(contract, 'ph'), 'PH'), 'Ph') from (select regexp_split_to_table(contract_no, '-') as contract from co_ntp_header where ntp_no = $P{ntp_no}) a where contract ~* 'ph')) as phase,
block::int,
/*
(case when array_length(lots) = 1 then format('LOT %s', lots[1]) else format('BETWEEN %s AND %s', lots[1], lots[array_length(lots)]) end) as lot,
*/
(
case
when $P{ntp_no} = '001672'
then format('LOT %s, %s, %s and %s', lots[1], lots[2], lots[3], lots[4])
else (case when array_length(lots) = 1 then format('LOT %s', lots[1]) else format('BETWEEN %s AND %s', lots[1], lots[array_length(lots)]) end)
end
) as lot,

a.work_desc,

a.amount

from (select seq_no,
	COALESCE(NULLIF(trim(split_part(split_part(work_desc, 'Phase', 2), 'Block', 1)), ''), '-') AS phase,
	trim(split_part(split_part(work_desc, (case when work_desc ~* 'block'then 'Block' else 'Blk' end), 2), 'Lot', 1)) as block,
	string_to_array(trim(unnest(string_to_array(replace(replace(replace(trim(split_part(initcap(work_desc), (case when work_desc ~* 'lots'then 'Lots' else 'Lot' end), 2)), 'And', ','), ' ', ''), '),(', ')~('), '~')), '()'), ',') as lots,
	work_desc,
	other_cost as amount
	from co_ntp_detail
	where ntp_no = $P{ntp_no}) a
order by seq_no, block::int, (CASE WHEN lots[1] ~* '-' THEN split_part(lots[1], '-', 1) ELSE lots[1] END)::int;]]>
	</queryString>
