	<queryString>
		<![CDATA[select

a.pmt_scheme_id,
trim(b.pmt_scheme_desc) as scheme_desc,
a.cm_scheme_id_internal,
trim(c.scheme_desc) as comm_sch_int,
a.cm_scheme_id_external,
trim(d.scheme_desc)  as comm_sch_ext


from rf_marketing_scheme_detail a
left join mf_payment_scheme b on a.pmt_scheme_id = b.pmt_scheme_id
left join cm_scheme_hd c on a.cm_scheme_id_internal = c.scheme_id
left join cm_scheme_hd d on a.cm_scheme_id_external = d.scheme_id

where a.rec_id = $P{rec_id}::int
and a.status_id = 'A'

order by a.pmt_scheme_id]]>
