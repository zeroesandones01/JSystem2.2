	<queryString>
		<![CDATA[select trim(b.part_desc) as part_desc, a.scheddate as sched_date, a.amount as amount, coalesce(a.proc_fee, 0.00) as proc_fee,
coalesce(a.mri, 0.00) as mri,coalesce(a.fire, 0.00) as fire,
coalesce(a.interest, 0.00) as interest, a.principal as principal,
coalesce(a.vat, 0.00) as vat, a.balance as balance,
coalesce(a.interest_rate, 0.00) as int_rate,
(select pmt_scheme_desc from mf_payment_scheme where pmt_scheme_id = $P{pmt_scheme}) as pmt_scheme
from rf_req_client_schedule a
left join mf_client_ledger_part b on b.part_id = a.part_id
where a.request_no = $P{request_no}
order by a.part_id, a.scheddate]]>
	</queryString>
