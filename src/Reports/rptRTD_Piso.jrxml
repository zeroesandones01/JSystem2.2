	<queryString>
		<![CDATA[SELECT A.c_project, B.Phase as c_phase, B.BLock as c_block, B.Lot as c_lot,
A.c_name, A.c_acct_no, A.c_date as c_date,
D.ave_balance as c_amount, 1 as c_final
FROM view_rtd_piso($P{05_CoID}, $P{06_Branch}, $P{03_dFrom}, $P{04_dTo}) A
LEFT JOIN mf_unit_info B ON TRIM(A.c_unit) = TRIM(B.description)
INNER JOIN rf_entity C ON A.c_name = C.entity_name
LEFT JOIN rf_rtd_accounts D ON C.entity_id = D.entity_id AND B.pbl_id = D.pbl_id
WHERE D.tobeprinted::INTEGER = 1::INTEGER
ORDER BY A.c_project, A.c_name]]>
	</queryString>
