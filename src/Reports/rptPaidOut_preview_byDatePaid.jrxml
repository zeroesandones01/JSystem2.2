	<queryString>
		<![CDATA[select

a.acct_id,
to_char(c.date_due,'MM-dd-yyyy') as check_date,
c.check_no,
a.cv_no,
to_char(b.cv_date,'MM-dd-yyyy') as dv_date,
to_char(b.date_paid,'MM-dd-yyyy') as date_paid,
trim(d.entity_name) as payee,
a.tran_amt as amount


from ( select * from rf_cv_detail where bal_side = 'C'
	and status_id = 'A'
	and case when $P{acct_id} = '' then acct_id is not null else acct_id = $P{acct_id} end
	) a
join ( select * from rf_cv_header where date_paid is not null and status_id = 'P' and proc_id in (5,6,7) ) b on a.cv_no = b.cv_no and a.co_id = b.co_id
left join (select * from rf_check where status_id = 'A') c on a.cv_no = c.cv_no and a.co_id = c.co_id
left join rf_entity d on coalesce(c.entity_id, b.entity_id1) = d.entity_id


where case when $P{pymnt_type} = '' then a.acct_id is not null else b.pay_type_id = $P{pymnt_type} end
and a.acct_id not in ( '00301004', '', '03-01-01-001' )
and case when $P{payee_id} = '' then b.entity_id1 is not null else coalesce(c.entity_id, b.entity_id1) = $P{payee_id} end
and case when $P{excl_payee_id} = '' then a.acct_id is not null else coalesce(c.entity_id, b.entity_id1) != $P{excl_payee_id} end
and a.co_id = $P{co_id}
and b.date_paid::date >= $P{date_from}::date
and b.date_paid::date <= $P{date_to}::date]]>
	</queryString>
