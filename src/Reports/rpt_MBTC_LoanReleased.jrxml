	<queryString>
		<![CDATA[SELECT TRIM(B.entity_name) AS Name, D.proj_name, C.Description,
(SELECT TRIM(dept_name) as sales_group FROM mf_department WHERE dept_code IN (SELECT TRIM(dept_id) FROM mf_sellingagent_info WHERE agent_id = e.sellingagent) AND status_id = 'A') as SalesGroup,
'PIMA' as PaymentParticular, 'WACK-WACK' as BankBranch, Now()::TIMESTAMP as DebitDate, TRIM(debit_amt::CHAR(10))::DECIMAL as DueAmt,
(
case
when (select count(*) from rf_hdmf_payments x where x.entity_id = a.entity_id and x.proj_id = a.projcode and x.pbl_id = a.pbl_id and x.seq_no::int = a.seq_no) > 0
then
(
select x.scheddate::date
from rf_hdmf_client_schedule x
where x.entity_id = a.entity_id and x.proj_id = a.projcode and x.pbl_id = a.pbl_id and x.seq_no = a.seq_no
and date_part('month', x.scheddate)::int = (select date_part('month', y.trans_date)::int from rf_hdmf_payments y where y.entity_id = a.entity_id and y.proj_id = a.projcode and y.pbl_id = a.pbl_id and y.seq_no::int = a.seq_no order by y.trans_date::Date desc limit 1)
and date_part('year', x.scheddate)::int = (select date_part('year', y.trans_date)::int from rf_hdmf_payments y where y.entity_id = a.entity_id and y.proj_id = a.projcode and y.pbl_id = a.pbl_id and y.seq_no::int = a.seq_no order by y.trans_date::Date desc limit 1)
order by x.scheddate asc limit 1
)
else
(
select x.scheddate
from rf_hdmf_client_schedule x
where x.entity_id = a.entity_id and x.proj_id = a.projcode and x.pbl_id = a.pbl_id and x.seq_no = a.seq_no
and date_part('month', x.scheddate)::int = date_part('month', now())::int
and date_part('year', x.scheddate)::int = date_part('year', now())::int
order by x.scheddate asc limit 1
)
end
) as dueDate
FROM rf_rtd_values_loanreleased A
INNER JOIN rf_entity B ON A.entity_id = B.entity_id
INNER JOIN mf_unit_info C ON A.projcode = C.proj_id AND A.pbl_id = C.pbl_id
INNER JOIN mf_project D ON A.projcode = D.proj_id
INNER JOIN rf_sold_unit E ON a.entity_id = e.entity_id and A.projcode = e.projcode and a.pbl_id = e.pbl_id and a.seq_no = e.seq_no and e.status_id != 'I'
WHERE A.status_id IN ('I', 'A')
ORDER BY B.entity_name]]>
