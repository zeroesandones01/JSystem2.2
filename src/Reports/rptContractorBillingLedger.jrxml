	<queryString>
		<![CDATA[select * from (

select

a.billing_no,
a.billing_type,
b.billingtype_alias,
to_char(a.billing_date,'MM-dd-yyyy') as billing_date,
a.accomp_pctg,
a.gross_amt,
a.vat_amt,
a.wtax_amt,
a.dp_recoupment,
a.retention_amt,
a.ca_liquidation,
a.other_deductions,
a.net_amt,
a.rplf_no,
to_char(d.date_paid,'MM-dd-yyyy') as date_paid,
e.contract_no,
trim(f.proj_name) as project,
(upper(trim(g.entity_name))||
(case when g.vat_registered is false then ' - Non-Vatable' else ' - Vatable' end)) as contractor,
--(case when e.contract_price is null then e.orig_contract_price else e.contract_price end) contract_price,
(case when e.contract_price is null and  e.new_contract_price is null then e.orig_contract_price
 	else (case when e.new_contract_price is null and e.contract_price is not null then e.contract_price else e.new_contract_price  end )  end) contract_price,
e.orig_contract_price,
e.dp_percentage,
(case when d.date_paid is not null and a.status_id != 'I'  then 'RELEASED' else
		(case when c.status_id = 'F' and a.status_id != 'I' then 'TAGGED' else
			(case when c.status_id = 'P' and a.status_id != 'I' then 'POSTED' else
				(case when c.status_id = 'I' then 'INACTIVE' else
					(case when cc.status_id = 'I' then 'INACTIVE' else
						(case when a.status_id = 'I' then 'INACTIVE' else
							(case when c.status_id = 'A' and a.status_id != 'I' then 'ACTIVE' else 'ACTIVE' end)
						 end)
					 end)
				 end)
			 end)
		 end)
	end)as status,
(case when c.status_id = 'P'  and c.reversal_date is not null then a.gross_amt else 0.00 end) as ded_gross,
(case when c.status_id = 'P'  and c.reversal_date is not null then a.vat_amt else 0.00 end) as ded_vat,
(case when c.status_id = 'P'  and c.reversal_date is not null then a.wtax_amt else 0.00 end) as ded_wtax,
(case when c.status_id = 'P'  and c.reversal_date is not null then a.dp_recoupment else 0.00 end) as ded_dp,
(case when c.status_id = 'P'  and c.reversal_date is not null then a.retention_amt else 0.00 end) as ded_ret,
(case when c.status_id = 'P'  and c.reversal_date is not null then a.ca_liquidation else 0.00 end) as ded_ca,
(case when c.status_id = 'P'  and c.reversal_date is not null then a.other_deductions else 0.00 end) as ded_OD,
(case when c.status_id = 'P'  and c.reversal_date is not null then a.net_amt else 0.00 end) as ded_net

from (select * from co_billing_detail where status_id != 'I') a
left join mf_billing_type b on a.billing_type = b.billingtype_id
left join rf_pv_header c on a.rplf_no = c.pv_no and a.co_id=c.co_id
left join rf_request_header cc on a.rplf_no = cc.rplf_no and a.co_id = cc.co_id
left join rf_cv_header d on c.cv_no = d.cv_no and c.co_id = d.co_id
left join co_ntp_header e on a.ntp_no = e.ntp_no
left join mf_project f on e.proj_id = f.proj_id
left join rf_entity g on e.entity_id = g.entity_id

where a.ntp_no = $P{ntp_no}
--and (case when c.status_id = 'P'  and c.reversal_date is not null then false else true end) -- Added by Erick DCRF 2143
order by a.billing_type, a.billing_date, a.accomp_pctg

) a where (billing_type, status) not in (select '02'::text, 'INACTIVE'::text)]]>
