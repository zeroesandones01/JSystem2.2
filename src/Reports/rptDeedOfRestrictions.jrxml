	<queryString>
		<![CDATA[SELECT sp_center_format(0, FORMAT('%s %s. %s', TRIM(c.first_name), substr(TRIM(c.middle_name), 1, 1), TRIM(c.last_name))) as client_name, get_client_address(a.entity_id) as client_address, d.proj_name, b.phase, b.block, b.lot, b.lotarea, b.floorarea, e.model_desc, e.model_class, e.with_color_scheme, f.company_name, f.company_address, g.*, f.company_logo

FROM rf_sold_unit a
LEFT JOIN mf_unit_info b ON b.proj_id = a.projcode and b.pbl_id = a.pbl_id
LEFT JOIN rf_entity c ON c.entity_id = a.entity_id
LEFT JOIN mf_project d ON d.proj_id = a.projcode
LEFT JOIN mf_product_model e ON e.model_id = a.model_id
LEFT JOIN mf_company f ON f.co_id = d.co_id
LEFT JOIN view_acknowledgement_part(a.entity_id, a.projcode, a.pbl_id, a.seq_no) g ON g.entity_id = a.entity_id

WHERE a.entity_id = $P{entity_id}
AND a.projcode = $P{proj_id}
AND a.pbl_id = $P{pbl_id}
AND a.seq_no = $P{seq_no};]]>
	</queryString>
