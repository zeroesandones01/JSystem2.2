	<queryString>
		<![CDATA[select distinct on (a.rplf_no, c.pcostdtl_desc, get_client_name(a.entity_id) ,a.pbl_id, a.pcostid_dl) a.pbl_id, b.phase, b.block, b.lot, trim(get_client_name(a.entity_id)) as buyer, a.pcost_amt, a.tran_date, trim(get_client_name(d.entity_id)) as requestedby, a.rplf_no, c.pcostdtl_desc as part_desc, a.pcostid_dl, a.batch_no,
$P{date_param}::date as date_param
from rf_processing_cost a
left join mf_unit_info b on b.pbl_id = a.pbl_id
LEFT join mf_processing_cost_dl c on c.pcostdtl_id = a.pcostid_dl
LEFT JOIN em_employee d on a.requested_by = d.emp_code
WHERE pcostdtl_id::int in (001, 034, 035, 036, 037, 002, 039, 038, 040, 041, 042, 043, 044, 003, 004, 045, 046, 005, 009, 019, 016, 015, 018, 017, 028, 025, 027, 026, 029, 032, 008, 007, 006, 011, 014, 012, 013, 010, 033, 022, 021, 024, 023, 020, 031, 030)
and (a.projcode = $P{projcode} or $P{projcode} = '')
and (b.phase = $P{phase} or $P{phase} = '')
AND a.tran_date::date between 'Fri Jan 01 00:00:00 PHT 2016'::date and $P{date_param}::date
AND a.status_id = 'A'
ORDER BY a.rplf_no, c.pcostdtl_desc, get_client_name(a.entity_id) ,a.pbl_id, a.pcostid_dl asc]]>
	</queryString>
