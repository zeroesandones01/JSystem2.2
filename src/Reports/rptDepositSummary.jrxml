	<queryString>
		<![CDATA[
select * from (

select
'DEPOSITS' as PDC, d.status_desc, a.dep_no, to_char(b.dep_date,'MM-dd-yyyy') as dep_date, to_char(b.cash_date,'MM-dd-yyyy') as cash_date, 'CASH' as type, f.bank_alias, '' as check_no, null as check_date, '' as part,  c.bank_acct_no, to_char(b.post_date, 'MM-dd-yyyy') as post_date, b.remarks, a.cash_amount
from (select distinct on (dep_no) dep_no, sum(value_id*dep_qty) as cash_amount from cs_dp_csh_detail where status_id = 'A' group by dep_no) a
left join cs_dp_header b on a.dep_no = b.dep_no
left join mf_bank_account c on b.bank_acct_id = c.bank_acct_id
left join mf_record_status d on b.status_id = d.status_id
left join mf_bank_branch e on c.bank_branch_id = e.bank_branch_id
left join mf_bank f on e.bank_id = f.bank_id
where (case when $P{date_used} = 'Collection Date :' then b.cash_date::date >= $P{date_from}::date and  b.cash_date::date <= $P{date_to}::date
else b.dep_date::date >= $P{date_from}::date and  b.dep_date::date <= $P{date_to}::date end)
and (case when $P{branch_name} = 'All' then b.dep_no is not null else b.branch_id = $P{branch_id} end)
and (case when $P{status_name} = 'All' then b.dep_no is not null else b.status_id = $P{status_id} end)

UNION ALL

select
(case when a.pay_rec_id in ( select pay_rec_id::int from rf_check_history where prev_checkstat_id = '04' and status_id = 'A' ) then 'PDC DEPOSITS' else 'DEPOSITS' end) as PDC,
d.status_desc, a.dep_no, to_char(b.dep_date,'MM-dd-yyyy') as dep_date, to_char(b.cash_date,'MM-dd-yyyy') as cash_date, 'CHECK' as type, f.bank_alias, a.check_no,
a.check_date, '' as part,  c.bank_acct_no, to_char(b.post_date, 'MM-dd-yyyy') as post_date, b.remarks, a.tran_amt
from (select * from cs_dp_chk_detail where status_id = 'A') a
left join cs_dp_header b on a.dep_no = b.dep_no
left join mf_bank_account c on b.bank_acct_id = c.bank_acct_id
left join mf_record_status d on b.status_id = d.status_id
left join mf_bank_branch e on c.bank_branch_id = e.bank_branch_id
left join mf_bank f on e.bank_id = f.bank_id
left join rf_payments g on a.pay_rec_id = g.pay_rec_id
left join mf_check_status h on g.check_stat_id = h.checkstat_id
where (case when $P{date_used} = 'Collection Date :' then b.cash_date::date >= $P{date_from}::date and  b.cash_date::date <= $P{date_to}::date
else b.dep_date::date >= $P{date_from}::date and  b.dep_date::date <= $P{date_to}::date end)
and (case when $P{proj_name} = 'All' then b.dep_no is not null else g.proj_id = $P{proj_id} end)
and (case when $P{branch_name} = 'All' then b.dep_no is not null else b.branch_id = $P{branch_id} end)
and (case when $P{status_name} = 'All' then b.dep_no is not null else b.status_id = $P{status_id} end)

) a

order by a.pdc, a.status_desc, a.type


]]>
