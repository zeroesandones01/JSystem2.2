	<queryString>
		<![CDATA[select a.asset_name, lpad(a.asset_no::varchar, 8, '0') as asset_no,
a.asset_code, d.entity_name
from tbl_asset a
left join tbl_asset_disposal b on a.asset_no = b.asset_no
left join em_employee c on a.current_cust::int = c.emp_code::int
left join rf_entity d on c.entity_id = d.entity_id
left join mf_office_branch e on a.loc_id = e.branch_id
where
(case when  $P{allbatch} = 'all'
	then b.batch is not null
		else (case when  $P{01_batch} =''
			then  b.batch is not null
				else  b.batch  = $P{01_batch} end)
end )
and ( case when $P{current_cust}::varchar = ''
	then (case when $P{allbatch} = 'all' then b.batch is not null  else b.batch = $P{01_batch} end)
		else a.current_cust::varchar =$P{current_cust}::varchar
end )
order by  d.entity_name, a.asset_name]]>
