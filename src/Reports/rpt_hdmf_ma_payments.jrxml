	<queryString>
		<![CDATA[select *
from
(
select a.*, (select x.proj_name from mf_project x where x.proj_id = a.c_proj_id) as project_name,
(select x.phase from mf_unit_info x where x.proj_id = a.c_proj_id and x.pbl_id = a.c_pbl_id) as phase,
1 as rowcount
from view_hdmf_collection($P{03_CoID}, $P{04_ProID}, $P{11_Phase}, $P{09_dteFrom}, $P{10_dteTo}, $P{12_RPLF}) a
where a.c_entity_name in (select z.client_name from tmp_hdmf z where z.emp_code = (select x.emp_code from em_employee x inner join rf_entity y on x.entity_id = y.entity_id where y.entity_name = $P{07_User})) and not exists (select x.* from rf_hdmf_payments x where x.remarks ~* 'Payment from MBTC' and x.remarks !~* 'Direct Deposit' and x.client_seqno = a.c_client_seqno)
) a
order by a.c_receipt_no, a.c_entity_name;]]>
	</queryString>
