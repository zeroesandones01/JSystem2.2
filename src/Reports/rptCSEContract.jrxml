	<parameter name="dp_amt" class="java.lang.String"/>
	<parameter name="dp_amt_words" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT a.orig_contract_price * (a.dp_percentage / 100)::numeric(19, 2) as dp_amt,
concat('DP', ' ', a.dp_percentage::int::varchar, '%') as dp_pctg,

replace(replace(concat('DP', ' ', a.dp_percentage::int::varchar, '% : ', (a.orig_contract_price * (a.dp_percentage / 100))::numeric(19, 2), ' (', cash_words((a.orig_contract_price * (a.dp_percentage / 100))::float8::numeric::money), '); '), ' dollars ', ' '), ' and ', ' ') as dp_words,

a.award_date,
a.contract_no,
trim(b.entity_name) as entity_name,
get_client_address_for2307(a.entity_id) as address,
initcap(trim(g.contactperson)) as contactperson,
trim(g.position) as position,
h.tin_no as tin,
initcap(trim(c.proj_name)) as proj_name,
trim(c.barangay) as proj_barangay,
trim(c.municipality) as proj_municipality,
trim(c.province) as proj_province,
d._count,
to_words(d._count::VARCHAR) AS  _word,
f.contract_price as orig_contract_price,
initcap(TRIM(REPLACE(split_part(cash_words(a.orig_contract_price::VARCHAR::MONEY), 'dollars', 1), ' and', ''))) AS orig_contract_price_word,
a.dp_percentage,
to_words(a.dp_percentage::VARCHAR) as dp_percentage_word,
(a.orig_contract_price * a.dp_percentage) /100 as downpayment,
initcap(TRIM(split_part(cash_words(((a.orig_contract_price * a.dp_percentage)/ 100)::VARCHAR::MONEY), 'dollars', 1))) as downpayment_word,
a.start_date,
a.finish_date,
date_part('day', timestamp_mi(timestamp_pl_interval(a.finish_date, INTERVAL '1 day'), a.start_date))::INT AS working_days,
initcap(to_words(date_part('day', timestamp_mi(timestamp_pl_interval(a.finish_date, INTERVAL '1 day'), a.start_date))::VARCHAR)) AS  working_days_word,
initcap(trim(e.company_name)) as company,
trim(e.company_address) as company_address,
(select (select initcap(sub_proj_name) from mf_sub_project where proj_id = a.proj_id and sub_proj_alias = a.phase group by initcap(sub_proj_name))
	from (select case when (sub_proj_id = '015' and proj_id = '019') THEN 'PH1-B' ELSE (SELECT regexp_split_to_table(contract_no, '-')) END as phase, proj_id
		from co_ntp_header
		where ntp_no = $P{ntp_no}) a
	where a.phase ~* 'PH') as phase,
(select sum(work_qty) from co_ntp_detail where ntp_no = $P{ntp_no}) as units,
a.ntp_type_id,
(select contract[array_length(contract)]
	from (select *, regexp_split_to_array(trim(contract_no), '-') as contract from co_ntp_header) a
	where ntp_no = $P{ntp_no}) as contract_type,

(select work_desc from co_ntp_detail where ntp_no = $P{ntp_no} and work_desc ~*'post' limit 1) as post,
(select distinct (work_desc ~* 'metering') as metering_center from co_ntp_detail where ntp_no = $P{ntp_no}) as metering_center,


(
	case
		when a.ntp_type_id = '03'
			then (select x.ntp_subject_description from co_ntp_subjects x where x.ntp_no = a.ntp_no)
		else (select ntp_header from co_cse_heading where ntp_type_id = a.ntp_type_id and is_metering = false)
	end
) as cse_heading

FROM co_ntp_header a
LEFT JOIN rf_entity b ON b.entity_id = a.entity_id
LEFT JOIN mf_project c ON c.proj_id = a.proj_id
LEFT JOIN (SELECT count(ntp_no) AS _count, ntp_no
	FROM co_ntp_detail
	WHERE ntp_no = $P{ntp_no}
	GROUP BY ntp_no) d ON d.ntp_no = a.ntp_no
LEFT JOIN mf_company e ON e.co_id = a.co_id
LEFT JOIN (SELECT ntp_no, sum(work_qty * other_cost) as contract_price
	FROM co_ntp_detail
	WHERE ntp_no = $P{ntp_no}
	GROUP BY ntp_no) f ON f.ntp_no = a.ntp_no
LEFT JOIN rf_contacts g ON g.entity_id = a.entity_id
LEFT JOIN rf_entity_id_no h ON h.entity_id = a.entity_id

WHERE a.ntp_no = $P{ntp_no};]]>
