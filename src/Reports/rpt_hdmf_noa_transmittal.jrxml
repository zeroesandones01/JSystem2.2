	<queryString>
		<![CDATA[SELECT c_name, c_phase || '-' || c_block || '-' || c_lot as unit, c_loanable_amount,
(select x.lotarea::int from rf_sold_unit x where x.entity_id = a.c_entity_id and x.projcode = a.c_proj_id and x.pbl_id = a.c_pbl_id and x.seq_no::int = a.c_seq_no::int and x.status_id = 'A') as lot_area,
c_model,
(select y.type_alias from rf_sold_unit x inner join mf_buyer_type y on x.buyertype = y.type_id where x.entity_id ~* a.c_entity_id and x.projcode = a.c_proj_id and x.pbl_id = a.c_pbl_id and x.seq_no::int = a.c_seq_no::int and x.status_id = 'A') as client_type,
c_noa_rlsd, c_filed_hdmf,
(select (SELECT TRIM(z.dept_name) as sales_group FROM mf_department z WHERE z.dept_code = x.mktgarm) from rf_sold_unit x where x.entity_id ~* a.c_entity_id and x.projcode = a.c_proj_id and x.pbl_id = a.c_pbl_id and x.seq_no::int = a.c_seq_no::int and x.status_id = 'A') as sales_group,
1::int as count
FROM view_hdmf_status_noareleased($P{co_id}, $P{project_id}, $P{phase}, $P{dateFrom}, $P{status},$P{batch}) a
where exists(select * from rf_buyer_status x where x.entity_id = a.c_entity_id and x.proj_id = a.c_proj_id and x.pbl_id = a.c_pbl_id and x.seq_no::int = a.c_seq_no::int and x.byrstatus_id = '35' and x.trans_no = $P{batch} and x.status_id = 'A')
ORDER BY A.c_status_sequence, A.c_name]]>
	</queryString>
