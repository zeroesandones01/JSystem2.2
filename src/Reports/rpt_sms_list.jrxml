	<queryString>
		<![CDATA[select b.entity_name as name, c.proj_alias as project, d.description as unit,
(select x.actual_date::date from rf_buyer_status x where x.entity_id = a.entity_id and x.proj_id = a.proj_id and x.pbl_id = a.pbl_id and x.seq_no::int = a.seq_no::int and x.status_id = 'A' and x.byrstatus_id = '32' limit 1) as loan_released, split_part(a.cellphone::varchar, ',', 1)::varchar AS cell1, split_part(a.cellphone::varchar, ',', 2)::varchar AS cell2, split_part(a.cellphone::varchar, ',', 3)::varchar AS cell3
from rf_sms_batch a
inner join rf_entity b on a.entity_id = b.entity_id
inner join mf_project c on a.proj_id = c.proj_id
inner join mf_unit_info d on a.proj_id = d.proj_id and a.pbl_id = d.pbl_id
where a.batch = $P{batch_no}
order by b.entity_name]]>
