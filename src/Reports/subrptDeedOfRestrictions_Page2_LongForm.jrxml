					<textElement textAlignment="Justified" verticalAlignment="Top" markup="styled">
						<font fontName="Arial" size="10"/>
					</textElement>
					<textFieldExpression class="java.lang.String"><![CDATA["From the date of execution of the Contract to Sell, the Buyer shall automatically become a Member of the Association."]]></textFieldExpression>
				</textField>
				<staticText>
					<reportElement x="49" y="227" width="27" height="12"/>
