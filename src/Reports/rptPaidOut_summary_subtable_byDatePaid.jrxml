<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="rptPaidOut_summary_subtable" pageWidth="504" pageHeight="792" columnWidth="448" leftMargin="28" rightMargin="28" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="acct_id" class="java.lang.String"/>
	<parameter name="payee_id" class="java.lang.String"/>
	<parameter name="excl_payee_id" class="java.lang.String"/>
	<parameter name="date_from" class="java.util.Date"/>
	<parameter name="date_to" class="java.util.Date"/>
	<parameter name="pymnt_type" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
to_char(a.date_paid,'MM-dd-yyyy') as date_paid,
sum(a.amount) as amount

from ( select

a.acct_id,
c.date_due,
c.check_no,
a.cv_no,
b.cv_date,
b.date_paid,
trim(d.entity_name) as payee,
a.tran_amt as amount,
e.bank_acct_no,
g.bank_name


from ( select * from rf_cv_detail where bal_side = 'C' and status_id = 'A' ) a
join ( select * from rf_cv_header where date_paid is not null and status_id = 'P' and proc_id in (5,6,7) ) b on a.cv_no = b.cv_no and a.co_id = b.co_id
join (select * from rf_check where status_id = 'A') c on a.cv_no = c.cv_no and a.co_id = c.co_id
left join rf_entity d on c.entity_id = d.entity_id
left join mf_bank_account e on a.acct_id = e.acct_id
left join mf_bank_branch f on e.bank_branch_id = f.bank_branch_id
left join mf_bank g on f.bank_id = g.bank_id

where case when $P{pymnt_type} = '' then a.acct_id is not null else b.pay_type_id = $P{pymnt_type} end
and a.acct_id not in ( '00301004', '', '03-01-01-001' )
and case when $P{acct_id} = '' then a.acct_id is not null else a.acct_id = $P{acct_id} end
and case when $P{payee_id} = '' then c.entity_id is not null else c.entity_id = $P{payee_id} end
and case when $P{excl_payee_id} = '' then a.acct_id is not null else c.entity_id != $P{excl_payee_id} end
and b.date_paid >= $P{date_from}::date
and b.date_paid <= $P{date_to}::date

) a

group by a.date_paid
order by a.date_paid]]>
	</queryString>
