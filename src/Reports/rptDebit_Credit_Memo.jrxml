	<queryString>
		<![CDATA[select
		a.db_no ,
		a.jv_no  ,
		a.tran_date ,
		a.tran_id  ,
		b.tran_desc ,
		c.status_desc ,
		e.status_desc ,
		a.tagged_by ,
		g.entity_name ,
		a.remarks,
		trim(h.dept_name) as dept_name,
		upper(i.company_name) as recipient_comp,
		upper(j.company_name) as comp,
		a.recipient_co_id,
		(case when a.memo_type = 'DM' then 'x' else '' end) as debit_memo,
		(case when a.memo_type = 'CM' then 'x' else '' end) as credit_memo,
		a.amount,
		l.entity_name as post_by_name,
		d.date_posted,
		j.company_address

		from rf_debit_credit_memo a
		left join mf_acctg_trans b on a.tran_id = b.tran_id
		left join mf_record_status c on a.status_id = c.status_id
		left join rf_jv_header d on a.jv_no = d.jv_no and a.co_id = d.co_id
		left join mf_record_status e on d.status_id = e.status_id
		left join em_employee f on a.tagged_by = f.emp_code
		left join rf_entity g on f.entity_id = g.entity_id
		left join mf_department h on f.dept_code = h.dept_code
		left join mf_company i on a.recipient_co_id = i.co_id
		left join mf_company j on a.co_id = j.co_id
		left join em_employee k on d.posted_by = k.emp_code
		left join rf_entity l on k.entity_id = l.entity_id


		where a.db_no = 2
		and a.co_id = '02'
		order by a.db_no desc]]>
	</queryString>
