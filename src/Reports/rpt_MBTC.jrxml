	<queryString>
		<![CDATA[/*
SELECT TRIM(B.entity_name) AS Name, D.proj_name, C.Description, '' as SalesGroup,
'DP' as PaymentParticular, 'WACK-WACK' as BankBranch, Now()::TIMESTAMP as DebitDate,
/*(SELECT SUM(x.c_amount) FROM view_card_dues(A.entity_id, A.projcode, A.pbl_id, A.seq_no, Now()::TIMESTAMP, FALSE) x) as DueAmt*/
TRIM(debit_amt::CHAR(10))::DECIMAL as DueAmt
FROM rf_rtd_values A
INNER JOIN rf_entity B ON A.entity_id = B.entity_id
INNER JOIN mf_unit_info C ON A.projcode = C.proj_id AND A.pbl_id = C.pbl_id
INNER JOIN mf_project D ON A.projcode = D.proj_id
WHERE A.status_id IN ('O', 'A')
ORDER BY B.entity_name
*/
SELECT TRIM(B.entity_name) AS Name, D.proj_name, C.Description,
 (SELECT TRIM(dept_name) as sales_group FROM mf_department WHERE dept_code IN (SELECT TRIM(dept_id) FROM mf_sellingagent_info WHERE agent_id = e.sellingagent) AND status_id = 'A') as SalesGroup,
'DP' as PaymentParticular, 'WACK-WACK' as BankBranch, Now()::TIMESTAMP as DebitDate,
/*(SELECT SUM(x.c_amount) FROM view_card_dues(A.entity_id, A.projcode, A.pbl_id, A.seq_no, Now()::TIMESTAMP, FALSE) x) as DueAmt*/
TRIM(debit_amt::CHAR(10))::DECIMAL as DueAmt,
(SELECT x.c_scheddate::DATE FROM view_card_dues(A.entity_id, A.projcode, A.pbl_id, A.seq_no, Now()::TIMESTAMP, FALSE) x LIMIT 1) as dueDate
FROM rf_rtd_values A
INNER JOIN rf_entity B ON A.entity_id = B.entity_id
INNER JOIN mf_unit_info C ON A.projcode = C.proj_id AND A.pbl_id = C.pbl_id
INNER JOIN mf_project D ON A.projcode = D.proj_id
INNER JOIN rf_sold_unit E ON a.entity_id = e.entity_id and A.projcode = e.projcode and a.pbl_id = e.pbl_id and a.seq_no = e.seq_no and e.status_id != 'I'
WHERE A.status_id IN ('O', 'A')
ORDER BY B.entity_name]]>
	</queryString>
