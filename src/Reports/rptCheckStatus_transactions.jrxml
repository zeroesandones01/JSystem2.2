	<queryString>
		<![CDATA[select

upper(trim(c.entity_name)) as name,
trim(f.description) as unit,
b.check_no,
a.trans_date,
trim(d.checkstat_desc) as prev_chek_stat,
trim(e.checkstat_desc) as new_chek_stat,
jv_no,
(upper(trim(h.last_name))||', '||substring(h.first_name,1,1)||'.') as created_by

from rf_check_history a
left join rf_payments b on a.pay_rec_id::int = b.pay_rec_id
left join rf_entity c on b.entity_id = c.entity_id
left join mf_check_status d on a.prev_checkstat_id = d.checkstat_id
left join mf_check_status e on a.new_checkstat_id = e.checkstat_id
left join mf_unit_info f on b.pbl_id = f.pbl_id
left join em_employee g on a.created_by  = g.emp_code
left join rf_entity h on g.entity_id = h.entity_id

where a.trans_date >= $P{date_from}
and a.trans_date - interval '1 day' <= $P{date_to}
and a.prev_checkstat_id != new_checkstat_id

order by a.trans_date]]>
	</queryString>
