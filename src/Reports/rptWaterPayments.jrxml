	<queryString>
		<![CDATA[SELECT to_char(a.reading_date,'MM-YYYY')  as  due_date, a.initial_meter_reading, a.meter_Reading, (a.meter_Reading-a.initial_meter_reading) as consumption,
a.reading_amt, COALESCE(a.garbage_fee, 0.00) AS garbage_fee,
(a.reading_amt + COALESCE(a.garbage_fee, 0.00)) as total,
to_char(b.tover_date,'MM-DD-YYYY') as turned_over_date,
to_char(b.movein_date,'MM-DD-YYYY') as movein_date,
unit.description
FROM rf_water_reading_v2 a
left join rf_qualified4orientation b on b.entity_id = a.entity_id and b.projcode = a.proj_id and b.pbl_id = a.pbl_id
left join mf_unit_info unit on a.proj_id = unit.proj_id and a.pbl_id = unit.pbl_id
and b.seq_no = a.seq_no and b.status_id = 'A'
WHERE a.entity_id = $P{entity_id}
and a.proj_id = $P{proj_id}
and a.pbl_id = $P{pbl_id}
and a.seq_no = $P{seq_no}
and a.status_id = 'A'
order by a.reading_date;]]>
	</queryString>
