	<queryString>
		<![CDATA[select row_number() over()::int as "row", now()::date as "Date", *,
(select x.doc_no from rf_tct_taxdec_monitoring_hd x
 inner join rf_tct_taxdec_monitoring_dl y on x.doc_no = y.doc_no
 where x.pbl_id = a.c_pbl_id
 and x.entity_id = a.c_entity_id
 and x.proj_id = a.c_proj_id
 and x.seq_no = a.c_seq_no
 and x.doc_type = '67'
 and y.doc_status = '212'
 and x.status_id = 'A'
 and y.status_id = 'A'
 ORDER BY x.date_created desc limit 1)
 as td_lot_no,

(select x.doc_no from rf_tct_taxdec_monitoring_hd x
 inner join rf_tct_taxdec_monitoring_dl y on x.doc_no = y.doc_no
 where x.pbl_id = a.c_pbl_id
 and x.entity_id = a.c_entity_id
 and x.proj_id = a.c_proj_id
 and x.seq_no = a.c_seq_no
 and x.doc_type = '98'
 and y.doc_status = '212'
 and x.status_id = 'A'
 and y.status_id = 'A'
 order by x.date_created
 limit 1)
 as td_house_no
from view_hdmf_rem_eleventh_officially_rem_v2($P{batch_no},$P{co_id},$P{proj_id},$P{phase})a
order by a.c_last_name, a.c_first_name, a.c_middle_name;]]>
