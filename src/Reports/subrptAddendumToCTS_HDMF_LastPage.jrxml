	<queryString>
		<![CDATA[SELECT --sp_center_format(30, FORMAT('%s %s', TRIM(c.first_name), TRIM(c.last_name))) as client_name,

sp_center_format(0, TRIM(f.company_name)) as company_name,
f.company_address,
sp_center_format(21, TRIM(d.proj_name)) as proj_name,
d.address as proj_address,
FORMAT('%s %s', d.proj_name, b.description) as proj_pbl,
sp_center_format(0, TRIM('JOHNNY LIWANAG CORPUZ')) as sir_johnny,
sp_center_format(52, FORMAT('%s %s. %s', TRIM(c.first_name), substr(TRIM(c.middle_name), 1, 1), TRIM(c.last_name))) as client_name,

sp_center_format(58, COALESCE(NULLIF(FORMAT('%s %s. %s', TRIM(g.connection_first_name), substr(TRIM(g.connection_middle_name), 1, 1), TRIM(g.connection_last_name)), ' '), 'N/A')) as client_spouse,

sp_center_format(135, COALESCE(get_client_address(a.entity_id), 'N/A')) as client_address,
sp_center_format(70, COALESCE(NULLIF(TRIM(FORMAT('%s %s', TRIM(k.connection_first_name), TRIM(k.connection_last_name))), ''), 'N/A')) as coapplicant,

sp_center_format(58, COALESCE(NULLIF(TRIM(FORMAT('%s %s', TRIM(m.connection_first_name), TRIM(m.connection_last_name))), ''), 'N/A')) as coapplicant_spouse,
sp_center_format(68, COALESCE(k.connection_address, 'N/A')) as coapplicant_address,

FORMAT('%s AND XX / 100', to_words(i.amount)) as tr_words,
i.amount as temporary_reservation,
FORMAT('%s AND XX / 100', to_words(j.amount)) as or_words,
j.amount as official_reservation,
sp_center_format(3 ,b.phase)as phase,
b.block, b.lot, b.lotarea, e.model_desc, a.sellingprice, o.client_id_no, o.client_id_presented, o.client_tin_no, o.signatory_name, o.signatory_id_presented, o.signatory_id_no, o.signatory_tin_no, o.signatory_position,
get_acts_no(a.entity_id, a.projcode, a.pbl_id, a.seq_no) as acts_no,
get_project_alias(a.projcode) as proj_alias,
FORMAT('%s-%s', b.block, b.lot) as blk_lt,
ROUND(b.floorarea::NUMERIC, 2) as floorarea,
sp_center_format(60, to_words(p.gross_sprice)) as tcp_words,
sp_center_format(100, TRIM(to_char(p.gross_sprice, '99,999,999D00'))) as tcp,
--p.gross_sprice as tcp,
sp_center_format(108, to_words(p.discount)) as discount_words,
sp_center_format(105, TRIM(to_char(p.discount, '99,999,990D00'))) as discount,
sp_center_format(108, to_words(COALESCE(p.vat, 0.00))) as vat_words,
sp_center_format(105, TRIM(to_char(COALESCE(p.vat, 0.00), '99,999,990D00'))) as vat,
sp_center_format(60, to_words(p.net_sprice)) as fcp_words,
sp_center_format(100, TRIM(to_char(p.net_sprice, '99,999,990D00'))) as fcp,
sp_center_format(56, to_words(q.dp+(q.dp_term*q.proc_fee))) as dp_outlay_words,
sp_center_format(16, TRIM(to_char((q.dp+(q.dp_term*q.proc_fee)), '99,999,990D00'))) as dp_outlay,

sp_center_format(15, trim(to_char(((q.dp/p.net_sprice)*100), '99,999,990D00'))) AS dp_rate,
sp_center_format(65, to_words(q.loanable_amount)) as ob_words,
sp_center_format(18, TRIM(to_char(q.loanable_amount, '99,999,990D00'))) as ob,


sp_center_format(56, to_words(r.res_fee1 + r.res_fee2)) as res_fee_words,
sp_center_format(20, trim(to_char( (r.res_fee1+r.res_fee2), '99,999,990DOO'))) AS res_fee,
sp_center_format(45, to_words((q.dp_term*q.proc_fee))) as misc_fee_words,
sp_center_format(16, TRIM(to_char((q.dp_term*q.proc_fee), '99,999,990DOO'))) as misc_fee,
k.connect_type


FROM rf_sold_unit a
LEFT JOIN mf_unit_info b ON b.proj_id = a.projcode and b.pbl_id = a.pbl_id
LEFT JOIN rf_entity c ON c.entity_id = a.entity_id
LEFT JOIN mf_project d ON d.proj_id = a.projcode
LEFT JOIN mf_product_model e ON e.model_id = a.model_id
LEFT JOIN mf_company f ON f.co_id = d.co_id

--Spouse
LEFT JOIN rf_entity_connect g ON g.entity_id = a.entity_id and g.connect_type IN ('SP', 'SC') AND g.status_id = 'A'

LEFT JOIN mf_pay_scheme_detail i ON i.pmt_scheme_id  = a.pmt_scheme_id AND i.part_id = '012' AND i.part_sequence = 1
LEFT JOIN mf_pay_scheme_detail j ON j.pmt_scheme_id  = a.pmt_scheme_id AND j.part_id = '012' AND j.part_sequence = 2

--Co-Applicant
LEFT JOIN rf_entity_connect k ON k.entity_id = a.entity_id AND k.connect_type IN ('CO', 'SC') AND k.status_id = 'A'

--Spouse
LEFT JOIN rf_entity_connect m ON m.entity_id = k.entity_id AND m.connect_type IN ('SP', 'SC') AND m.status_id = 'A'

LEFT JOIN (SELECT * FROM view_acknowledgement_part($P{entity_id}, $P{proj_id}, $P{pbl_id}, $P{seq_no})) o ON o.entity_id = a.entity_id
LEFT JOIN rf_client_price_history p on p.entity_id = a.entity_id and p.proj_id = a.projcode and p.pbl_id = a.pbl_id and p.seq_no = a.seq_no AND p.status_id = 'A'
LEFT JOIN rf_pagibig_computation q on q.entity_id = a.entity_id and q.proj_id = a.projcode and q.pbl_id = a.pbl_id and q.seq_no = a.seq_no AND q.status_id = 'A'
LEFT JOIN mf_product_model r on r.model_id = a.model_id

WHERE a.entity_id = $P{entity_id}
AND a.projcode = $P{proj_id}
AND a.pbl_id = $P{pbl_id}
AND a.seq_no = $P{seq_no};]]>
