get_client_name(a.entity_id) as "Client Name",
a.rplf_no as "RPLF",
sum(a.tcost_amt) as "Amount",
(SELECT b.acct_name as "Account Description" FROM mf_boi_chart_of_accounts b where b.acct_id = (SELECT acct_id from rf_request_detail where rplf_no = a.rplf_no limit 1))  ,
get_client_name(a.payee) as "Payee",
a.retrn_check_no as "Returned Check Number",
NULLIF(TRIM(a.date_edited), 'null')::DATE as "Date Returned"

from rf_transfer_cost a
WHERE (CASE WHEN $P{p_co_id} IS NULL THEN TRUE ELSE a.co_id = $P{p_co_id} END)
AND (CASE WHEN $P{p_rplf_no} IS NULL THEN TRUE ELSE a.rplf_no = $P{p_rplf_no} END)
GROUP by a.entity_id, a.projcode, a.pbl_id, a.seq_no, a.rplf_no, a.payee, a.retrn_check_no, NULLIF(TRIM(a.date_edited), 'null')::DATE;]]>
