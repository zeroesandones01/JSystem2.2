	<queryString>
		<![CDATA[Select get_client_name(a.entity_id) as client,
	c.company_alias as company,
	b.proj_alias as project,
	d.description as unit,
	a.date_printed::DATE as date_printed,
	(Select received_date::DATE from dm_docs_inout
		where entity_id = a.entity_id
		and proj_id = a.proj_id
		and  pbl_id = a.pbl_id
		and seq_no = a.seq_no
		and status_id = 'A'
		and doc_id ='274'
		and inout_code = false
		ORDER by rec_id DESC limit 1) as docs_in_date
	from
	rf_printed_documents a
	INNER JOIN mf_project b ON b.proj_id = a.proj_id and b.status_id = 'A'
	INNER JOIN mf_company c ON c.co_id = b.co_id
	LEFT JOIN mf_unit_info d ON d.proj_id = a.proj_id AND d.pbl_id = a.pbl_id
	where a.doc_id = '309' and a.status_id = 'A'
	AND CASE WHEN NULLIF($P{co_id}, '') IS NULL THEN TRUE ELSE c.co_id = $P{co_id} end
	AND CASE WHEN NULLIF($P{proj_id}, '') IS NULL THEN TRUE ELSE a.proj_id = $P{proj_id} end
	AND CASE WHEN NULLIF($P{phase}, '') IS NULL THEN TRUE ELSE d.phase = $P{phase} end
AND a.date_printed::DATE BETWEEN $P{date_from} AND $P{date_to};]]>
	</queryString>
