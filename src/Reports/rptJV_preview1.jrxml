	<queryString>
		<![CDATA[select

coalesce(a.acct_id,'') as acct_id,
coalesce(b.division_alias,'')||
case when b.division_alias is null or c.dept_alias is null then ' ' else '-' end ||
coalesce(c.dept_alias,'')||' / '||coalesce(d.proj_alias,'')|| '-'||
-- case when trim(e.phase) is null or trim(e.phase) = '' then '' else  '-PH'||coalesce(trim(e.phase),'') end as cost_center,
coalesce((select sub_proj_alias from mf_sub_project where trim(proj_id) = trim(a.project_id) and trim(sub_proj_id) = trim(a.sub_projectid) AND status_id = 'A'),'') as cost_center,

f.acct_name,
a.ref_no,
( case when a.bal_side = 'D' then a.tran_amt else null end) as debit,
( case when a.bal_side = 'D' then null else a.tran_amt end) as credit,
'/'||trim(i.entity_name) as prepared_by

from (select * from rf_jv_detail where status_id not in  ('I', 'D')) a
left join mf_division b on a.div_id = b.division_code
left join mf_department c on a.dept_id = c.dept_code
left join mf_project d on a.project_id = d.proj_id and a.co_id = d.co_id
--left join mf_sub_project e on trim(a.project_id) = trim(e.proj_id) and TRIM(a.sub_projectid) = trim(e.sub_proj_id)
left join mf_boi_chart_of_accounts f on a.acct_id = f.acct_id
left join mf_unit_info g on a.pbl_id = g.pbl_id and a.project_id = g.proj_id
left join em_employee h on (case when a.edited_by is null or a.edited_by = '' then a.created_by else a.edited_by end) = h.emp_code
left join rf_entity i on h.entity_id = i.entity_id

where a.jv_no = $P{jv_no}
and a.co_id = $P{co_id}


/*order by a.line_no, a.bal_side desc, a.pbl_id*/
order by a.bal_side desc, a.line_no]]>
